moreTab:AddSection("[ÊÑõ] - Efeitos da Interface")
local rainbowTitleAtivo, rainbowBgAtivo = false, false
local function findRedzWindow() for _, c in ipairs(game:GetService("CoreGui"):GetChildren()) do if c:IsA("ScreenGui") and c:FindFirstChild("Window") then return c.Window end end end
moreTab:AddToggle({ Name = "T√≠tulo em Arco-√çris", Default = false, Callback = function(state) rainbowTitleAtivo = state end })
moreTab:AddToggle({ Name = "Fundo em Arco-√çris", Default = false, Callback = function(state) rainbowBgAtivo = state end })
RunService.RenderStepped:Connect(function()
    pcall(function()
        local window = findRedzWindow()
        if not window then return end
        local topBar, title = window:FindFirstChild("Top"), nil
        if topBar then title = topBar:FindFirstChild("Title") end
        if rainbowTitleAtivo and title then title.TextColor3 = Color3.fromHSV((tick() % 5) / 5, 0.8, 1) end
        if rainbowBgAtivo and window then window.BackgroundColor3 = Color3.fromHSV((tick() % 8) / 8, 1, 0.4) end
    end)
end)

