--======================================================================================--
--//                      A CHAVE MESTRA v2.0 (TELEPORTE CIR√öRGICO)                      //
--======================================================================================--
playerTab:AddSection("üåÄ Habilidades de Onipresen√ßa")

--// O c√©rebro da nossa chave mestra.
local chaveMestraConfig = {
    ativo = false,
    alvoAtual = nil -- Onde a gente guarda o prompt que t√° na mira do mouse
}
local promptsAtivos = {}

--// A UI DE INTERA√á√ÉO √Ä DIST√ÇNCIA.
local interacaoGui = Instance.new("ScreenGui", CoreGui); interacaoGui.Name="DRIP_InteracaoUI"; interacaoGui.ResetOnSpawn=false; interacaoGui.Enabled=false
local btnInteragir = Instance.new("TextButton", interacaoGui); btnInteragir.Size=UDim2.fromOffset(200,40); btnInteragir.Position=UDim2.new(0.5,-100,0.8,0); btnInteragir.BackgroundColor3=Color3.fromRGB(0,255,255); btnInteragir.Text="[E] INTERAGIR √Ä DIST√ÇNCIA"; btnInteragir.Font=Enum.Font.GothamBold; btnInteragir.TextColor3=Color3.new(0,0,0)
Instance.new("UICorner", btnInteragir).CornerRadius = UDim.new(0,8)

--// O GATILHO DO TELEPORTE
btnInteragir.MouseButton1Click:Connect(function()
    if chaveMestraConfig.alvoAtual then
        pcall(function()
            local myHRP = LocalPlayer.Character.HumanoidRootPart
            local posOriginal = myHRP.CFrame

            -- VAI
            myHRP.CFrame = chaveMestraConfig.alvoAtual.Parent.CFrame + Vector3.new(0,3,0)
            task.wait() -- Espera um frame pro jogo registrar
            
            -- AGE
            fireproximityprompt(chaveMestraConfig.alvoAtual)
            task.wait()

            -- VOLTA
            myHRP.CFrame = posOriginal
        end)
    end
end)


--// O TOGGLE PRINCIPAL NO NOSSO HUB
playerTab:AddToggle({
    Name = "Ativar Onipresen√ßa (Interagir √† Dist√¢ncia)",
    Default = false,
    Callback = function(state)
        chaveMestraConfig.ativo = state
        if not state then
            for prompt, data in pairs(promptsAtivos) do
                pcall(function()
                    prompt.Enabled=data.originalEnabled; prompt.RequiresLineOfSight=data.originalLineOfSight; prompt.MaxActivationDistance=data.originalDistance
                    if data.gui then data.gui:Destroy() end
                end)
            end
            promptsAtivos = {}
            interacaoGui.Enabled = false
        end
    end
})
--// O SCANNER E ARROMBADOR
task.spawn(function()
    while task.wait(0.2) do
        if not chaveMestraConfig.ativo then continue end
        
        pcall(function()
            -- L√≥gica para achar e for√ßar os prompts (igual a antes)
            for _, prompt in ipairs(workspace:GetDescendants()) do
                if prompt:IsA("ProximityPrompt") then
                    if not promptsAtivos[prompt] then
                        promptsAtivos[prompt]={originalEnabled=prompt.Enabled,originalLineOfSight=prompt.RequiresLineOfSight,originalDistance=prompt.MaxActivationDistance,gui=nil}
                    end
                    prompt.Enabled=true; prompt.RequiresLineOfSight=false; prompt.MaxActivationDistance=9e9
                end
            end
            
            -- L√≥gica do mouse pra mostrar o bot√£o de interagir
            local mouse = LocalPlayer:GetMouse()
            local target = mouse.Target and mouse.Target.Parent and mouse.Target.Parent:FindFirstChildOfClass("ProximityPrompt")

            if target and promptsAtivos[target] then
                interacaoGui.Enabled = true
                chaveMestraConfig.alvoAtual = target
            else
                interacaoGui.Enabled = false
                chaveMestraConfig.alvoAtual = nil
            end
        end)
    end
end)
playerTab:AddSection("‚ö° Habilidades de Velocidade")

--// O c√©rebro da nossa m√£o invis√≠vel.
local maoDeMidasConfig = {
    ativo = false
}

--// O Toggle que ativa a tua velocidade divina.
playerTab:AddToggle({
    Name = "Intera√ß√£o Instant√¢nea (Auto Interact)",
    Default = false,
    Callback = function(state)
        maoDeMidasConfig.ativo = state
    end
})

--// A ESCUTA TELEF√îNICA (A TUA L√ìGICA, CHEFE, PORQUE √â PERFEITA)
game:GetService("ProximityPromptService").PromptButtonHoldBegan:Connect(function(prompt)
    -- Se o poder estiver ativo, a gente age.
    if maoDeMidasConfig.ativo then
        -- A gente nem pergunta, a gente dispara a porra toda.
        pcall(function()
            fireproximityprompt(prompt)
        end)
    end
end)

--// SE√á√ÉO DE VOO
playerTab:AddSection("[ÊÑõ] -Ô∏è Voo Universal")
local voando, velocidadeVoo, vooConexao = false, 50, nil
playerTab:AddToggle({ Name = "Ativar Voo (PC/Mobile)", Default = false, Callback = function(estado)
    voando = estado
    if not voando then
        if vooConexao then vooConexao:Disconnect(); vooConexao = nil end
        pcall(function()
            local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
            if hrp then
                if hrp:FindFirstChild("FlyGyro") then hrp.FlyGyro:Destroy() end
                if hrp:FindFirstChild("FlyVelocity") then hrp.FlyVelocity:Destroy() end
            end
        end)
        return
    end
    task.spawn(function()
        local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
        local hrp, humanoid = char:WaitForChild("HumanoidRootPart"), char:WaitForChild("Humanoid")
        if not (hrp and humanoid) then return end
        local gyro = Instance.new("BodyGyro", hrp); gyro.Name = "FlyGyro"; gyro.MaxTorque = Vector3.new(9e9, 9e9, 9e9); gyro.P = 20000
        local velocity = Instance.new("BodyVelocity", hrp); velocity.Name = "FlyVelocity"; velocity.MaxForce = Vector3.new(9e9, 9e9, 9e9); velocity.Velocity = Vector3.new(0, 0, 0)
        vooConexao = RunService.RenderStepped:Connect(function()
            if not voando or not humanoid or humanoid.Health <= 0 then
                if vooConexao then vooConexao:Disconnect(); vooConexao = nil end
                pcall(function() gyro:Destroy(); velocity:Destroy() end)
                return
            end
            gyro.CFrame = camera.CFrame
            local direcaoVertical = humanoid.Jump and velocidadeVoo or 0
            local direcaoMovimento = humanoid.MoveDirection * velocidadeVoo
            velocity.Velocity = Vector3.new(direcaoMovimento.X, direcaoVertical, direcaoMovimento.Z)
        end)
    end)
end })
playerTab:AddSlider({ Name = "Velocidade do Voo", Min = 10, Max = 500, Default = 50, Callback = function(v) velocidadeVoo = v end })

--[=[
    DRIP NO MODO COMA INDUZIDO! üõå
    A morte n√£o √© o fim. √â um camarote.
    Controle quando e SE tu vai voltar a viver.
]=]

--======================================================================================--
--//                      COMA INDUZIDO (PERMANECER MORTO)                               //
--======================================================================================--
playerTab:AddSection("üõå Habilidades de Morte")

--// O c√©rebro do nosso coveiro.
local comaConfig = {
    ativo = false,
    respawnTimeOriginal = Players.RespawnTime
}

local respawnConnection = nil

--// O Toggle que te bota no caix√£o.
playerTab:AddToggle({
    Name = "Permanecer Morto (Modo Espectador)",
    Default = false,
    Callback = function(state)
        comaConfig.ativo = state
        
        if state then
            -- A gente j√° aumenta o tempo de respawn pra infinito.
            Players.RespawnTime = math.huge
        else
            -- Se desligou, a gente volta o tempo ao normal e for√ßa o respawn.
            Players.RespawnTime = comaConfig.respawnTimeOriginal
            pcall(function() LocalPlayer:LoadCharacter() end)
        end
    end
})

--// O VIGIA DA MORTE
local function vigiarMorte(char)
    local humanoid = char:WaitForChild("Humanoid")
    
    -- Desconecta qualquer vigia antigo pra n√£o dar merda.
    if respawnConnection then respawnConnection:Disconnect() end

    respawnConnection = humanoid.Died:Connect(function()
        -- Se o poder t√° ligado QUANDO A GENTE MORRE...
        if comaConfig.ativo then
            task.wait(0.1) -- Espera a tela de morte do Roblox tentar aparecer
            pcall(function()
                -- A GENTE MANDA ELA TOMAR NO CU.
                local respawnGui = CoreGui:FindFirstChild("RobloxGui", true)
                if respawnGui then
                    respawnGui.RespawnFrame.Visible = false
                end
            end)
        end
    end)
end

-- Conecta o vigia quando o personagem aparece.
LocalPlayer.CharacterAdded:Connect(vigiarMorte)
-- Se o personagem j√° existe, conecta o vigia nele.
if LocalPlayer.Character then
    vigiarMorte(LocalPlayer.Character)
end


--// SE√á√ÉO DE UTILIDADES DE MOVIMENTO
playerTab:AddSection("[ÊÑõ] -Ô∏è Utilit√°rios de Movimento")
local noclipAtivo, jumpInfinitoAtivo, escalando = false, false, false
local noclipConexao, jumpConexao, escaladaConexao = nil, nil, nil
playerTab:AddToggle({ Name = "Noclip (Atravessar Paredes)", Default = false, Callback = function(state)
    noclipAtivo = state
    if noclipAtivo then
        noclipConexao = RunService.Stepped:Connect(function()
            if not noclipAtivo then noclipConexao:Disconnect(); noclipConexao = nil; return end
            pcall(function() for _, part in pairs(LocalPlayer.Character:GetDescendants()) do if part:IsA("BasePart") then part.CanCollide = false end end end)
        end)
    elseif noclipConexao then noclipConexao:Disconnect(); noclipConexao = nil end
end })
playerTab:AddToggle({ Name = "Pulo Infinito", Default = false, Callback = function(state)
    jumpInfinitoAtivo = state
    if jumpInfinitoAtivo then
        jumpConexao = UserInputService.JumpRequest:Connect(function() pcall(function() LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping) end) end)
    elseif jumpConexao then jumpConexao:Disconnect(); jumpConexao = nil end
end })
local plataformaAtiva = false
