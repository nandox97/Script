playerTab:AddSection("‚ûø Circuito Infinito")

--// O c√©rebro do nosso circuito.
local circuitoConfig = {
    ativo = false,
    posicaoItem1 = nil,
    posicaoItem2 = nil
}

--// A interface do nosso painel de controle.
local statusItem1 = playerTab:AddParagraph({ "Posi√ß√£o Item 1:", "N√£o definida" })
local statusItem2 = playerTab:AddParagraph({ "Posi√ß√£o Item 2:", "N√£o definida" })

playerTab:AddButton({
    Name = "MEMORIZAR ITEM 1 (Mire com o Mouse)",
    Callback = function()
        pcall(function()
            local mouse = LocalPlayer:GetMouse()
            if mouse.Target and mouse.Target.Parent then
                circuitoConfig.posicaoItem1 = mouse.Target.CFrame
                statusItem1:SetText({"Posi√ß√£o Item 1:", "DEFINIDA!"})
                print("DRIP: Posi√ß√£o do Item 1 memorizada.")
            else
                warn("DRIP: Mire em alguma coisa, caralho.")
            end
        end)
    end
})

playerTab:AddButton({
    Name = "MEMORIZAR ITEM 2 (Mire com o Mouse)",
    Callback = function()
        pcall(function()
            local mouse = LocalPlayer:GetMouse()
            if mouse.Target and mouse.Target.Parent then
                circuitoConfig.posicaoItem2 = mouse.Target.CFrame
                statusItem2:SetText({"Posi√ß√£o Item 2:", "DEFINIDA!"})
                print("DRIP: Posi√ß√£o do Item 2 memorizada.")
            else
                warn("DRIP: Mire em alguma coisa, caralho.")
            end
        end)
    end
})

playerTab:AddToggle({
    Name = "Ativar Loop de Coleta",
    Default = false,
    Callback = function(state)
        circuitoConfig.ativo = state

        if state then
            if not circuitoConfig.posicaoItem1 or not circuitoConfig.posicaoItem2 then
                warn("DRIP: Tu precisa memorizar a porra dos DOIS itens antes de ligar o loop.")
                circuitoConfig.ativo = false
                -- A gente precisa de uma forma de desmarcar o toggle na UI,
                -- mas por enquanto a l√≥gica de seguran√ßa t√° aqui.
                return
            end

            -- O LOOP DA GAN√ÇNCIA
            task.spawn(function()
                local proximoAlvo = 1
                while circuitoConfig.ativo do
                    pcall(function()
                        local myHRP = LocalPlayer.Character and LocalPlayer.Character.HumanoidRootPart
                        if not myHRP then return end
                        
                        if proximoAlvo == 1 then
                            myHRP.CFrame = circuitoConfig.posicaoItem1
                            proximoAlvo = 2
                        else
                            myHRP.CFrame = circuitoConfig.posicaoItem2
                            proximoAlvo = 1
                        end
                        
                        -- SIMULA O CLIQUE PRA COLETAR
                        vim:SendKeyEvent(true, Enum.KeyCode.E, false, game)
                        task.wait(0.05)
                        vim:SendKeyEvent(false, Enum.KeyCode.E, false, game)
                    end)
                    task.wait(0.5) -- Um delay pra dar tempo do item respawnar. Ajuste se precisar.
                end
            end)
        end
    end
})

