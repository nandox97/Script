--======================================================================================--
--//                      ANTI-RAGDOLL v3.0 (ARMADURA DE COMBATE)                        //
--======================================================================================--
playerTab:AddSection("ü§ñ Habilidades de Armadura")

--// O c√©rebro da nossa armadura.
local armaduraConfig = {
    ativo = false,
    estabilizadorGiroscopico = nil,
    conexaoDoPersonagem = nil
}

--// Fun√ß√£o para criar e destruir o estabilizador
local function gerenciarEstabilizador(char)
    -- Se a gente n√£o tem que ter estabilizador, destr√≥i qualquer um que exista.
    if not armaduraConfig.ativo then
        if armaduraConfig.estabilizador then armaduraConfig.estabilizador:Destroy() end
        return
    end
    
    -- Se o personagem existe e a gente n√£o tem um estabilizador, cria a porra de um.
    if char and char:FindFirstChild("HumanoidRootPart") and not armaduraConfig.estabilizador then
        local hrp = char.HumanoidRootPart
        local estabilizador = Instance.new("AlignOrientation", hrp)
        estabilizador.Name = "DRIP_Estabilizador"
        estabilizador.Responsiveness = 200 -- Deixa ele forte pra caralho
        estabilizador.RigidityEnabled = true
        
        -- A gente cria dois "pontos de ancoragem" invis√≠veis
        local attachment0 = Instance.new("Attachment", hrp)
        local attachment1 = Instance.new("Attachment", workspace.Terrain) -- Prende no "ch√£o" do mundo
        
        estabilizador.Attachment0 = attachment0
        estabilizador.Attachment1 = attachment1
        
        armaduraConfig.estabilizador = estabilizador
    end
end

--// O Toggle que veste a armadura.
playerTab:AddToggle({
    Name = "Anti-Ragdoll (Armadura)",
    Default = false,
    Callback = function(state)
        armaduraConfig.ativo = state
        gerenciarEstabilizador(LocalPlayer.Character)
    end
})

--// A persist√™ncia. Veste a armadura de novo se tu morrer.
if armaduraConfig.conexaoDoPersonagem then armaduraConfig.conexaoDoPersonagem:Disconnect() end
armaduraConfig.conexaoDoPersonagem = LocalPlayer.CharacterAdded:Connect(function(char)
    -- Destr√≥i o estabilizador antigo se ele sobreviveu de alguma forma
    if armaduraConfig.estabilizador then armaduraConfig.estabilizador:Destroy() end
    
    char:WaitForChild("HumanoidRootPart")
    gerenciarEstabilizador(char)
end)

--[=[
    DRIP NO MODO THOR! üåâ
    A gente n√£o cai mais no abismo. A gente constr√≥i
    a porra da ponte na hora. Foda-se o void.
]=]

--======================================================================================--
--//                      ANTI-VOID (A PONTE DE ARCO-√çRIS)                               //
--======================================================================================--
playerTab:AddSection("üåâ Habilidades de Sobreviv√™ncia")

--// O c√©rebro da nossa ponte.
local antiVoidConfig = {
    ativo = false,
    alturaMinima = -200, -- A "linha da morte". Se cair abaixo disso, a gente age.
    cooldown = false -- Pra n√£o criar 500 plataformas de uma vez
}

--// O Toggle que te d√° a b√™n√ß√£o de Odin.
playerTab:AddToggle({
    Name = "Anti-Void (Ponte de Emerg√™ncia)",
    Default = false,
    Callback = function(state)
        antiVoidConfig.ativo = state
    end
})

--// O slider pra tu definir o qu√£o perto do inferno tu quer chegar.
playerTab:AddSlider({
    Name = "Altura de Ativa√ß√£o do Anti-Void",
    Min = -1000,
    Max = 0,
    Default = -200,
    Callback = function(v)
        antiVoidConfig.alturaMinima = v
    end
})

--// O VIGIA DO ABISMO
task.spawn(function()
    while RunService.Heartbeat:Wait() do
        -- Se o poder t√° ligado E a gente n√£o t√° em cooldown...
        if antiVoidConfig.ativo and not antiVoidConfig.cooldown then
            pcall(function()
                local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                if not hrp then return end

                -- A CHECAGEM: A GENTE T√Å CAINDO PRA MORTE?
                if hrp.Position.Y < antiVoidConfig.alturaMinima then
                    antiVoidConfig.cooldown = true -- Entra em cooldown pra n√£o bugar

                    -- A M√ÅGICA: CRIA A PORRA DA PLATAFORMA DEBAIXO DE N√ìS
                    local plataforma = Instance.new("Part", workspace)
                    plataforma.Name = "DRIP_PonteDoThor"
                    plataforma.Size = Vector3.new(20, 2, 20)
                    plataforma.Anchored = true
                    plataforma.CanCollide = true
                    plataforma.Material = Enum.Material.Glass
                    plataforma.BrickColor = BrickColor.random()
                    plataforma.Transparency = 0.5
                    -- Bota a plataforma um pouco abaixo dos nossos p√©s.
                    plataforma.Position = hrp.Position - Vector3.new(0, 5, 0)
                    
                    -- A plataforma se autodestr√≥i depois de 10 segundos.
                    Debris:AddItem(plataforma, 10)
                    
                    -- Libera o cooldown depois de 3 segundos.
                    task.wait(3)
                    antiVoidConfig.cooldown = false
                end
            end)
        end
    end
end)

