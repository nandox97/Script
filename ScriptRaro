trollTab:AddSection("üåç Editor de Realidade")

--// O c√©rebro do nosso arquiteto.
local editorConfig = {
    ativo = false,
    alvoSelecionado = nil,
    conexaoDoMouse = nil
}

--// A UI DE CONTROLE QUE VAI APARECER NA TELA.
local editorGui = Instance.new("ScreenGui", CoreGui); editorGui.Name = "DRIP_EditorUI"; editorGui.ResetOnSpawn = false; editorGui.Enabled = false
local painelDeEdicao = Instance.new("Frame", editorGui); painelDeEdicao.Size = UDim2.fromOffset(150, 200); painelDeEdicao.Position = UDim2.new(0, 10, 0.5, -100); painelDeEdicao.BackgroundColor3 = Color3.fromRGB(30,30,30); painelDeEdicao.BackgroundTransparency = 0.2; Instance.new("UICorner", painelDeEdicao).CornerRadius = UDim.new(0,8)
local layoutVertical = Instance.new("UIListLayout", painelDeEdicao); layoutVertical.Padding = UDim.new(0, 5); layoutVertical.HorizontalAlignment = Enum.HorizontalAlignment.Center; layoutVertical.SortOrder = Enum.SortOrder.LayoutOrder
local tituloPainel = Instance.new("TextLabel", painelDeEdicao); tituloPainel.Size = UDim2.new(1, 0, 0, 30); tituloPainel.BackgroundColor3 = Color3.fromRGB(170,0,255); tituloPainel.Text = "EDI√á√ÉO"; tituloPainel.Font = Enum.Font.GothamBold; tituloPainel.TextColor3 = Color3.new(1,1,1)
--// FUN√á√ÉO PRA CRIAR BOT√ÉO, S√ì PRA N√ÉO REPETIR C√ìDIGO QUE NEM UM ANIMAL
local function criarBotaoDeEdicao(nome, texto)
    local btn = Instance.new("TextButton", painelDeEdicao); btn.Name = nome; btn.Size = UDim2.new(1, -10, 0, 30); btn.BackgroundColor3 = Color3.fromRGB(80,80,80); btn.Text = texto; btn.Font = Enum.Font.Gotham; btn.TextColor3 = Color3.new(1,1,1)
    Instance.new("UICorner", btn).CornerRadius = UDim.new(0,4)
    return btn
end

local btnDeletar = criarBotaoDeEdicao("Deletar", "DELETAR")
local btnInvisivel = criarBotaoDeEdicao("Invisivel", "INVIS√çVEL / VIS√çVEL")
local btnSemColisao = criarBotaoDeEdicao("SemColisao", "COLIS√ÉO ON / OFF")
local btnAumentar = criarBotaoDeEdicao("Aumentar", "AUMENTAR TAMANHO")
local btnDiminuir = criarBotaoDeEdicao("Diminuir", "DIMINUIR TAMANHO")

--// A L√ìGICA DOS BOT√ïES
btnDeletar.MouseButton1Click:Connect(function() if editorConfig.alvoSelecionado then editorConfig.alvoSelecionado:Destroy(); editorConfig.alvoSelecionado = nil; editorGui.Enabled = false end end)
btnInvisivel.MouseButton1Click:Connect(function() if editorConfig.alvoSelecionado then editorConfig.alvoSelecionado.Transparency = 1 - editorConfig.alvoSelecionado.Transparency end end)
btnSemColisao.MouseButton1Click:Connect(function() if editorConfig.alvoSelecionado then editorConfig.alvoSelecionado.CanCollide = not editorConfig.alvoSelecionado.CanCollide end end)
btnAumentar.MouseButton1Click:Connect(function() if editorConfig.alvoSelecionado then editorConfig.alvoSelecionado.Size = editorConfig.alvoSelecionado.Size * 1.2 end end)
btnDiminuir.MouseButton1Click:Connect(function() if editorConfig.alvoSelecionado then editorConfig.alvoSelecionado.Size = editorConfig.alvoSelecionado.Size * 0.8 end end)


--// O TOGGLE NO NOSSO HUB QUE ATIVA A VARA DE COND√ÉO
trollTab:AddToggle({
    Name = "Ativar Editor de Realidade",
    Default = false,
    Callback = function(state)
        editorConfig.ativo = state
        local mouse = LocalPlayer:GetMouse()

        if state then
            editorConfig.conexaoDoMouse = mouse.Button1Down:Connect(function()
                if not mouse.Target or mouse.Target.Name == "DRIP_AlmaPenada" then -- N√£o deixa a gente se selecionar
                    editorGui.Enabled = false
                    return
                end
                
                editorConfig.alvoSelecionado = mouse.Target
                editorGui.Enabled = true -- Mostra o painel de edi√ß√£o
            end)
        else
            if editorConfig.conexaoDoMouse then editorConfig.conexaoDoMouse:Disconnect() end
            editorGui.Enabled = false
            editorConfig.alvoSelecionado = nil
        end
    end
})
--======================================================================================--
--//                   X. RECHEANDO A ABA 'TROLL' (CLONAGEM FE)                         //
--======================================================================================--
pcall(function()
    -- O c√©rebro do nosso Puxador (l√≥gica clonada)
    local puxadorConfig = {
        ativo = false,
        distancia = 5,
        jogadoresSelecionados = {}
    }

    trollTab:AddSection("[ÊÑõ] - Puxar players (Puxador FE)")

    -- // CLONAGEM DO PLAYERDROPDOWN E PLAYERLISTFRAME
    -- A gente vai usar um Dropdown da nossa UI pra fazer o mesmo trampo.
    local playerOptions = {"Ningu√©m"}
    local playerDropdown = trollTab:AddDropdown({
        Name = "Alvo do Reboque",
        Options = playerOptions,
        Default = "Ningu√©m",
        Callback = function(selecionado)
            puxadorConfig.jogadoresSelecionados = {} -- Limpa a lista antes de escolher de novo

            if selecionado == "Todos" then
                for _, p in ipairs(Players:GetPlayers()) do
                    if p ~= LocalPlayer then table.insert(puxadorConfig.jogadoresSelecionados, p) end
                end
            elseif selecionado == "Time" then
                if LocalPlayer.Team then
                    for _, p in ipairs(Players:GetPlayers()) do
                        if p.Team == LocalPlayer.Team and p ~= LocalPlayer then table.insert(puxadorConfig.jogadoresSelecionados, p) end
                    end
                end
            elseif selecionado == "Inimigos" then
                for _, p in ipairs(Players:GetPlayers()) do
                    if p.Team ~= LocalPlayer.Team then table.insert(puxadorConfig.jogadoresSelecionados, p) end
                end
            elseif selecionado ~= "Ningu√©m" then
                local p = Players:FindFirstChild(selecionado)
                if p then table.insert(puxadorConfig.jogadoresSelecionados, p) end
            end
        end
    })

    -- Bot√£o pra atualizar a lista de jogadores no Dropdown
    trollTab:AddButton({
        Name = "ATUALIZAR LISTA DE ALVOS",
        Callback = function()
            local novasOpcoes = {"Ningu√©m", "Todos", "Time", "Inimigos"}
            for _, p in ipairs(Players:GetPlayers()) do
                if p ~= LocalPlayer then table.insert(novasOpcoes, p.Name) end
            end
            playerDropdown:Set(novasOpcoes)
        end
    })

    -- // CLONAGEM DO DISTANCETEXTBOX
    -- Um Slider √© mais a nossa cara, mais seguro e mais f√°cil de usar.
    trollTab:AddSlider({
        Name = "Dist√¢ncia do Pux√£o",
        Min = 1,
        Max = 100,
        Default = 5,
        Callback = function(v)
            puxadorConfig.distancia = v
        end
    })

    -- // CLONAGEM DO STARTBUTTON
    -- Um Toggle √© perfeito pra isso. Liga, o loop come√ßa. Desliga, o loop para.
    local startToggle = trollTab:AddToggle({
        Name = "Ativar Puxador de Ot√°rio",
        Default = false,
        Callback = function(v)
            puxadorConfig.ativo = v
            
            if v then
                -- O CORA√á√ÉO DA TUA L√ìGICA, CHEFE. O LOOP.
                task.spawn(function()
                    while puxadorConfig.ativo do
                        pcall(function()
                            if not LocalPlayer.Character or not LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then return end
                            
                            local myHRP = LocalPlayer.Character.HumanoidRootPart
                            local lookVector = myHRP.CFrame.lookVector
                            local posAlvo = myHRP.Position + (lookVector * puxadorConfig.distancia)
                            
                            -- Puxa cada jogador na nossa lista de selecionados
                            for _, alvo in ipairs(puxadorConfig.jogadoresSelecionados) do
                                if alvo and alvo.Character and alvo.Character:FindFirstChild("HumanoidRootPart") then
                                    local alvoHRP = alvo.Character.HumanoidRootPart
                                    -- A gente usa o m√©todo bruto pra garantir
                                    alvo.Character:SetPrimaryPartCFrame(CFrame.new(posAlvo))
                                end
                            end
                        end)
                        task.wait(0.1) -- Delay pra n√£o explodir o jogo
                    end
                end)
            end
        end
    })

end)

trollTab:AddSection("[ÊÑõ] - Freezer de Ot√°rio")
local freezerConfig = { ativo = false, alvo = nil, posicaoSalva = nil }
local freezerDropdown = trollTab:AddDropdown({ Name = "SELECIONAR V√çTIMA", Options = {}, Callback = function(v) freezerConfig.alvo = Players:FindFirstChild(v) end })
trollTab:AddButton({ Name = "ATUALIZAR LISTA", Callback = function() local l={}; for _,p in ipairs(Players:GetPlayers()) do if p ~= LocalPlayer then table.insert(l, p.Name) end end; pcall(function() freezerDropdown:SetOptions(l) end) end })
trollTab:AddToggle({ Name = "Congelar Alvo", Default = false, Callback = function(state)
    freezerConfig.ativo = state
    if state and freezerConfig.alvo and freezerConfig.alvo.Character and freezerConfig.alvo.Character:FindFirstChild("HumanoidRootPart") then
        freezerConfig.posicaoSalva = freezerConfig.alvo.Character.HumanoidRootPart.CFrame
    else
        freezerConfig.posicaoSalva = nil
    end
end })
RunService.Heartbeat:Connect(function()
    if freezerConfig.ativo and freezerConfig.alvo and freezerConfig.posicaoSalva then
        pcall(function() freezerConfig.alvo.Character.HumanoidRootPart.CFrame = freezerConfig.posicaoSalva end)
    end
end)

