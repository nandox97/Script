UserInputService.InputBegan:Connect(function(input)
    if plataformaAtiva and input.KeyCode == Enum.KeyCode.LeftShift then
        pcall(function()
            local myHRP = LocalPlayer.Character.HumanoidRootPart
            local plat = Instance.new("Part", workspace); plat.Size = Vector3.new(8, 1, 8); plat.Anchored = true; plat.BrickColor = BrickColor.random()
            plat.CFrame = myHRP.CFrame - Vector3.new(0, 4, 0); Debris:AddItem(plat, 10)
        end)
    end
end)

--// SE√á√ÉO DE HABILIDADES SOBRE-HUMANAS
playerTab:AddSection("[ÊÑõ] - Habilidades Sobre-Humanas")
local jesusAtivo = false
playerTab:AddToggle({ Name = "Andar sobre a √Ågua", Default = false, Callback = function(state) jesusAtivo = state end })
RunService.Heartbeat:Connect(function()
    if jesusAtivo then
        pcall(function()
            local myChar = LocalPlayer.Character
            local ray = Ray.new(myChar.HumanoidRootPart.Position, Vector3.new(0, -5, 0))
            local _, pos = workspace:FindPartOnRayWithIgnoreList(ray, {myChar})
            if workspace.Terrain:GetMaterial(pos) == Enum.Material.Water then
                myChar.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
                myChar.HumanoidRootPart.CFrame = CFrame.new(myChar.HumanoidRootPart.Position.X, workspace.Terrain.WaterWaveHeight, myChar.HumanoidRootPart.Position.Z)
            end
        end)
    end
end)
playerTab:AddToggle({ Name = "Baixa Gravidade (Hip Jump)", Default = false, Callback = function(state) workspace.Gravity = state and 75 or 196.2 end })
