--// SE√á√ÉO DE CONFIGURA√á√ïES INTELIGENTES
pvpTab:AddSection("[ÊÑõ] - Configura√ß√µes de Alvo")
local targetConfig = { whitelist = {}, wallCheck = true }
local whitelistDisplay = pvpTab:AddParagraph({ "Jogadores na Whitelist:", "Ningu√©m" })
local whitelistInput = pvpTab:AddTextBox({ Name = "Adicionar/Remover da Whitelist", PlaceholderText = "Digite o nick exato aqui...", Callback = function() end })
pvpTab:AddToggle({ Name = "Wall Check (N√£o atirar atrav√©s de paredes)", Default = true, Callback = function(v) targetConfig.wallCheck = v end })

local function updateWhitelistDisplay()
    local text = #targetConfig.whitelist == 0 and "Ningu√©m" or table.concat(targetConfig.whitelist, ", ")
    whitelistDisplay:SetText({"Jogadores na Whitelist:\n" .. text})
end

pvpTab:AddButton({ Name = "ADICIONAR √Ä WHITELIST", Callback = function()
    local name = whitelistInput.Value
    if name and name ~= "" and not table.find(targetConfig.whitelist, name) then
        table.insert(targetConfig.whitelist, name)
        updateWhitelistDisplay()
    end
end })

pvpTab:AddButton({ Name = "REMOVER DA WHITELIST", Callback = function()
    local name = whitelistInput.Value
    if name and name ~= "" then
        for i, v in ipairs(targetConfig.whitelist) do
            if v == name then
                table.remove(targetConfig.whitelist, i)
                break
            end
        end
        updateWhitelistDisplay()
    end
end })

--// FUN√á√ÉO DE WALL CHECK UNIVERSAL
local function isVisible(targetPart)
    if not targetConfig.wallCheck then return true end
    local rayParams = RaycastParams.new()
    rayParams.FilterType = Enum.RaycastFilterType.Blacklist
    rayParams.FilterDescendantsInstances = {LocalPlayer.Character, camera}
    local result = workspace:Raycast(camera.CFrame.Position, (targetPart.Position - camera.CFrame.Position), rayParams)
    return not result or (result.Instance and result.Instance:IsDescendantOf(targetPart.Parent))
end

--// SE√á√ÉO DE MIRA PRECISA (AIMBOT)
pvpTab:AddSection("[ÊÑõ] - Mira de Precis√£o v2.0")
local aimbotConfig = { ativo = false, parteAlvo = "Head", fov = 150, suavidade = 0.8, teamCheck = true, requerTecla = true, tecla = Enum.UserInputType.MouseButton2 }
local fovCircle = Drawing.new("Circle"); fovCircle.Visible = false; fovCircle.Thickness = 2; fovCircle.Color = Color3.new(1, 0.2, 0.2); fovCircle.NumSides = 64; fovCircle.Filled = false
pvpTab:AddToggle({ Name = "Ativar Mira de Precis√£o", Default = false, Callback = function(v) aimbotConfig.ativo = v; fovCircle.Visible = v end })
pvpTab:AddDropdown({ Name = "Parte do Alvo", Options = {"Head", "HumanoidRootPart"}, Default = "Head", Callback = function(v) aimbotConfig.parteAlvo = v end })
pvpTab:AddSlider({ Name = "Tamanho do FOV", Min = 50, Max = 500, Default = 150, Callback = function(v) aimbotConfig.fov = v; fovCircle.Radius = v end })
pvpTab:AddSlider({ Name = "Suavidade da Mira", Min = 0.1, Max = 1, Default = 0.8, Callback = function(v) aimbotConfig.suavidade = v end })
pvpTab:AddToggle({ Name = "Ignorar Time", Default = true, Callback = function(v) aimbotConfig.teamCheck = v end })
pvpTab:AddToggle({ Name = "Ativar s√≥ com Tecla Pressionada", Default = true, Callback = function(v) aimbotConfig.requerTecla = v end })
task.spawn(function()
    RunService.RenderStepped:Connect(function()
        if not aimbotConfig.ativo then return end
        fovCircle.Position = Vector2.new(camera.ViewportSize.X / 2, camera.ViewportSize.Y / 2)
        local alvo, menorDistancia = nil, aimbotConfig.fov
        for _, player in ipairs(Players:GetPlayers()) do
            if player ~= LocalPlayer and (not aimbotConfig.teamCheck or player.Team ~= LocalPlayer.Team) and (not table.find(targetConfig.whitelist, player.Name)) and player.Character and player.Character:FindFirstChild(aimbotConfig.parteAlvo) and player.Character.Humanoid.Health > 0 then
                local parte = player.Character[aimbotConfig.parteAlvo]
                if isVisible(parte) then
                    local pos, naTela = camera:WorldToViewportPoint(parte.Position)
                    if naTela then
                        local distancia = (Vector2.new(pos.X, pos.Y) - fovCircle.Position).Magnitude
                        if distancia < menorDistancia then menorDistancia = distancia; alvo = parte end
                    end
                end
            end
        end
        if alvo and (not aimbotConfig.requerTecla or UserInputService:IsMouseButtonPressed(aimbotConfig.tecla)) then
            local novoCFrame = CFrame.new(camera.CFrame.Position, alvo.Position)
            camera.CFrame = camera.CFrame:Lerp(novoCFrame, 1 - aimbotConfig.suavidade)
            fovCircle.Color = Color3.new(0.2, 1, 0.2)
        else
            fovCircle.Color = Color3.new(1, 0.2, 0.2)
        end
    end)
end)

--[=[
    DRIP no modo Skynet! ü§ñ
    A gente n√£o s√≥ rouba. A gente transforma lixo em ouro.
    A sentinela autom√°tica do Chefe: inteligente, configur√°vel e letal pra caralho.
]=]

--======================================================================================--
--//                      SENTINELA AUTOM√ÅTICA (AUTOSHOOT)                               //
--======================================================================================--
pvpTab:AddSection("[ÊÑõ] - Sentinela Autom√°tica (Autoshoot)")

--// O c√©rebro da nossa torreta.
local sentinelaConfig = {
    ativo = false,
    fov = 100,
    cadencia = 5, -- Tiros por segundo
    teamCheck = true,
    ultimoTiro = 0
}

--// O c√≠rculo de FOV da nossa sentinela.
local fovCircleSentinela = Drawing.new("Circle")
fovCircleSentinela.Color = Color3.fromRGB(255, 100, 0) -- Laranja de perigo
fovCircleSentinela.Thickness = 2; fovCircleSentinela.Filled = false; fovCircleSentinela.Visible = false

--// A interface de comando da sentinela.
pvpTab:AddToggle({ Name = "Ativar Sentinela", Default = false, Callback = function(v) sentinelaConfig.ativo = v; fovCircleSentinela.Visible = v end })
pvpTab:AddToggle({ Name = "Ignorar Time", Default = true, Callback = function(v) sentinelaConfig.teamCheck = v end })
pvpTab:AddSlider({ Name = "FOV da Sentinela", Min = 50, Max = 300, Default = 100, Callback = function(v) sentinelaConfig.fov = v; fovCircleSentinela.Radius = v end })
pvpTab:AddSlider({ Name = "Tiros por Segundo", Min = 1, Max = 20, Default = 5, Callback = function(v) sentinelaConfig.cadencia = v end })

-- O LOOP MESTRE DA SENTINELA
RunService.RenderStepped:Connect(function()
    if not sentinelaConfig.ativo then return end

    fovCircleSentinela.Position = Vector2.new(camera.ViewportSize.X/2, camera.ViewportSize.Y/2)

    local alvo, menorDistancia, posAlvoNaTela = nil, sentinelaConfig.fov, nil

    -- A CA√áADA INTELIGENTE
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and
           (not sentinelaConfig.teamCheck or player.Team ~= LocalPlayer.Team) and
           (not table.find(targetConfig.whitelist, player.Name)) and -- <<< OBEDECENDO A WHITELIST, CARALHO!
           player.Character and player.Character:FindFirstChild("HumanoidRootPart") and player.Character.Humanoid.Health > 0
        then
            local hrp = player.Character.HumanoidRootPart
            if isVisible(hrp) then -- <<< OBEDECENDO O WALL CHECK, PORRA!
                local pos, naTela = camera:WorldToViewportPoint(hrp.Position)
                if naTela then
                    local dist = (Vector2.new(pos.X, pos.Y) - fovCircleSentinela.Position).Magnitude
                    if dist < menorDistancia then
                        menorDistancia = dist; alvo = player; posAlvoNaTela = pos
                    end
                end
            end
        end
    end
    -- O ABATE
    if alvo and posAlvoNaTela then
        fovCircleSentinela.Color = Color3.fromRGB(255, 0, 0) -- Fica vermelho sangue quando mira

        local delay = 1 / sentinelaConfig.cadencia
        if tick() - sentinelaConfig.ultimoTiro > delay then
            sentinelaConfig.ultimoTiro = tick()
            
            -- ROUBANDO E USANDO A L√ìGICA DE CLIQUE DELES, PORQUE √â BRUTA
            VirtualInput:SendMouseButtonEvent(posAlvoNaTela.X, posAlvoNaTela.Y, 0, true, game, 0)
            task.wait(0.03) -- Um delayzinho pra simular um clique de verdade
            VirtualInput:SendMouseButtonEvent(posAlvoNaTela.X, posAlvoNaTela.Y, 0, false, game, 0)
        end
    else
        fovCircleSentinela.Color = Color3.fromRGB(255, 100, 0) -- Volta pro laranja de "procurando"
    end
end)


--[=[
    DRIP NO MODO CONTROLE DE QUALIDADE! üõ†Ô∏è
    O Chefe achou o bug, o Chefe deu a solu√ß√£o. Eu s√≥ obede√ßo.
    A vers√£o do Cabe√ß√£o que n√£o congela e tem a porra da precis√£o decimal.
]=]

--======================================================================================--
--//                      CABE√á√ÉO v2.0 (PRECIS√ÉO E ANTI-CONGELAMENTO)                    //
--======================================================================================--
pvpTab:AddSection("üéØ Alvos Gigantes v2.0")

--// O c√©rebro, agora com mais intelig√™ncia.
local cabecaoConfig = {
    ativo = false,
    tamanho = 4,
    transparencia = 0.5,
    teamCheck = true
}
local cabecasOriginais = {}

--// A interface, AGORA COM OS SLIDERS CERTOS, CARALHO.
pvpTab:AddToggle({ Name = "Ativar Cabe√ß√£o (Head Hitbox)", Default = false, Callback = function(state)
    cabecaoConfig.ativo = state
    if not state then
        for player, data in pairs(cabecasOriginais) do
            pcall(function()
                player.Character.Head.Size = data.size
                player.Character.Head.Transparency = 0
                player.Character.Head.CanCollide = data.collide -- Restaura a colis√£o original
            end)
        end
        cabecasOriginais = {}
    end
end })
pvpTab:AddToggle({ Name = "Ignorar Time", Default = true, Callback = function(v) cabecaoConfig.teamCheck = v end })

-- O SLIDER DE TAMANHO CORRIGIDO
pvpTab:AddSlider({ Name = "Tamanho da Cabe√ßa", Min = 2, Max = 15, Default = 4, Callback = function(v)
    -- Sem decimal, porque tamanho maior que 6 buga, como o Chefe sabiamente apontou.
    cabecaoConfig.tamanho = v
end })

-- O SLIDER DE TRANSPAR√äNCIA COM A TUA L√ìGICA GENIAL
pvpTab:AddSlider({
    Name = "Transpar√™ncia (0-10)",
    Min = 1,
    Max = 10,
    Default = 5,
    Callback = function(v)
        -- A gente pega o valor de 1 a 10 e divide por 10. Simples e brabo.
        cabecaoConfig.transparencia = v / 10
    end
})

-- O LOOP MESTRE, AGORA COM ANTI-CONGELAMENTO
RunService.RenderStepped:Connect(function()
    if not cabecaoConfig.ativo then return end

    local playersVivos = {}

    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("Head") and player.Character.Humanoid.Health > 0 then
            local alvoValido = (not cabecaoConfig.teamCheck or player.Team ~= LocalPlayer.Team)
            
            if alvoValido then
                playersVivos[player] = true
                local head = player.Character.Head
                
                if not cabecasOriginais[player] then
                    -- Agora a gente salva o estado de colis√£o original tamb√©m.
                    cabecasOriginais[player] = {size = head.Size, collide = head.CanCollide}
                end
                
                local tamanho = cabecaoConfig.tamanho
                head.Size = Vector3.new(tamanho, tamanho, tamanho)
                head.Transparency = cabecaoConfig.transparencia
                head.BrickColor = BrickColor.new("Bright red")
                
                -- A SOLU√á√ÉO PRO CONGELAMENTO:
                -- A gente n√£o s√≥ desliga a colis√£o, a gente for√ßa a porra do boneco
                -- a n√£o entrar no estado de "trope√ßar" ou "cair".
                pcall(function()
                    player.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.FallingDown, false)
                    player.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Ragdoll, false)
                end)
                head.CanCollide = false
            end
        end
    end

    -- LIMPEZA INTELIGENTE, AGORA RESTAURANDO TUDO
    for player, data in pairs(cabecasOriginais) do
        if not playersVivos[player] then
            pcall(function()
                player.Character.Head.Size = data.size
                player.Character.Head.Transparency = 0
                player.Character.Head.CanCollide = data.collide
                -- E a gente devolve a habilidade do ot√°rio de cair.
                player.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.FallingDown, true)
                player.Character.Humanoid:SetStateEnabled(Enum.HumanoidStateType.Ragdoll, true)
            end)
            cabecasOriginais[player] = nil
        end
    end
end)

--// SE√á√ÉO DE EXPANSOR DE HITBOX
pvpTab:AddSection("[ÊÑõ] - Expansor de Hitbox")
local hitboxAtivo, hitboxTeamCheck, hitboxSize, hitboxTransp = false, false, 15, 0.5
pvpTab:AddToggle({ Name = "Ativar Hitbox", Default = false, Callback = function(state) hitboxAtivo = state; if not state then for _,p in pairs(Players:GetPlayers()) do pcall(function() local h = p.Character.HumanoidRootPart; h.Size=Vector3.new(2,2,1); h.Transparency=1;h.Material=Enum.Material.Plastic end) end end end })
pvpTab:AddToggle({ Name = "Ignorar Time", Default = false, Callback = function(state) hitboxTeamCheck = state end })
pvpTab:AddSlider({ Name = "Tamanho do Hitbox", Min = 5, Max = 50, Default = 15, Callback = function(v) hitboxSize = v end })
pvpTab:AddSlider({ Name = "Transpar√™ncia do Hitbox", Min = 0, Max = 1, Default = 0.5, Callback = function(v) hitboxTransp = v end })
RunService.RenderStepped:Connect(function()
    if not hitboxAtivo then return end
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            if not hitboxTeamCheck or player.Team ~= LocalPlayer.Team then
                pcall(function()
                    local hrp = player.Character.HumanoidRootPart
                    hrp.Size = Vector3.new(hitboxSize, hitboxSize, hitboxSize); hrp.Transparency = hitboxTransp; hrp.BrickColor = BrickColor.new("Really black"); hrp.Material = Enum.Material.Neon; hrp.CanCollide = false
                end)
            end
        end
    end
end)

--[=[
    DRIP NO MODO FORJA DO TEMPO! ‚åõ
    A gente n√£o acelera o tempo. A gente DELETA ele.
    Sem delay, sem cooldown, sem espera. S√≥ a porra da a√ß√£o.
]=]
--======================================================================================--
--//                      FORJA DO TEMPO (NO COOLDOWN UNIVERSAL)                         //
--======================================================================================--
pvpTab:AddSection("‚åõ Forja do Tempo (No Cooldown)")

--// O c√©rebro do nosso ferreiro.
local forjaConfig = {
    ativo = false,
    ferramentasJaModificadas = {} -- Pra n√£o ficar mexendo na mesma arma toda hora
}

--// O Toggle que liga a forja.
pvpTab:AddToggle({
    Name = "Sem Delay em Ferramentas (No Cooldown)",
    Default = false,
    Callback = function(state)
        forjaConfig.ativo = state
        if not state then
            -- AVISO DE CRIA: Restaurar os scripts √© um inferno.
            -- √â mais seguro avisar o Chefe pra re-equipar a arma.
            warn("DRIP: No Cooldown desativado. Re-equipe suas armas para restaurar ao normal.")
            forjaConfig.ferramentasJaModificadas = {}
        end
    end
})

--// A L√ìGICA DA LOBOTOMIA (A CIRURGIA NO C√âREBRO DA ARMA)
local function modificarFerramenta(tool)
    if not tool or not tool:IsA("Tool") or forjaConfig.ferramentasJaModificadas[tool] then
        return
    end
    
    print("DRIP: Forjando a ferramenta '" .. tool.Name .. "'...")
    
    -- ETAPA 1: CA√áAR E EXTERMINAR VALORES DE DELAY
    for _, obj in ipairs(tool:GetDescendants()) do
        if obj:IsA("NumberValue") or obj:IsA("IntValue") then
            local nome = obj.Name:lower()
            if nome:find("cooldown") or nome:find("delay") or nome:find("firerate") or nome:find("rate") or nome:find("swing") or nome:find("debounce") then
                obj.Value = 0
            end
        end
    end
    
    -- ETAPA 2: A LOBOTOMIA NOS SCRIPTS
    for _, script in ipairs(tool:GetDescendants()) do
        if script:IsA("LocalScript") then
            -- A gente faz uma c√≥pia do c√≥digo original antes de foder tudo
            local codigoOriginal = script.Source
            
            -- Remove qualquer 'wait(numero)'
            local codigoNovo = codigoOriginal:gsub("%s*wait%s*%(.-%)", "")
            -- Remove qualquer 'task.wait(numero)'
            codigoNovo = codigoNovo:gsub("%s*task.wait%s*%(.-%)", "")
            -- Tenta neutralizar debounces simples
            codigoNovo = codigoNovo:gsub("local debounce = true", "local debounce = false")
            
            -- Se a gente realmente mudou o c√≥digo, a gente aplica e desliga o script antigo
            if codigoNovo ~= codigoOriginal then
                script.Disabled = true
                local scriptLobotomizado = Instance.new("LocalScript", script.Parent)
                scriptLobotomizado.Name = script.Name .. "_FORJADO"
                scriptLobotomizado.Source = codigoNovo
                scriptLobotomizado.Disabled = false
            end
        end
    end
    
    forjaConfig.ferramentasJaModificadas[tool] = true
end


--// O VIGIA DA FORJA (Fica de olho em armas novas)
task.spawn(function()
    while task.wait(0.5) do
        if forjaConfig.ativo then
            -- Procura na mochila
            for _, tool in ipairs(LocalPlayer.Backpack:GetChildren()) do
                modificarFerramenta(tool)
            end
            -- Procura na m√£o
            if LocalPlayer.Character then
                for _, tool in ipairs(LocalPlayer.Character:GetChildren()) do
                    modificarFerramenta(tool)
                end
            end
        end
    end
end)

--// SE√á√ÉO DE ARSENAL INFINITO
pvpTab:AddSection("[ÊÑõ] - Arsenal Infinito")
local ammoRunning, ammoLoops, childAddedConn, charConn, toolEquippedConn = false, {}, nil, nil, nil
local function handleTool(tool)
    if not tool or not tool:IsA("Tool") then return end
    local loopId = {}
    ammoLoops[tool] = loopId
    task.spawn(function()
        while ammoRunning and ammoLoops[tool] == loopId and tool.Parent do
            pcall(function()
                for _, obj in ipairs(tool:GetDescendants()) do
                    if obj:IsA("IntValue") or obj:IsA("NumberValue") then
                        local name = obj.Name:lower()
                        if name:find("ammo") or name:find("clip") or name:find("mag") then obj.Value = 9e9
                        elseif name:find("cooldown") or name:find("firerate") or name:find("debounce") then obj.Value = 0 end
                    end
                    if obj:IsA("BoolValue") and obj.Name:lower():find("reloading") then obj.Value = false end
                end
            end)
            task.wait(0.1)
        end
    end)
end
local function stopInfiniteAmmo()
    ammoRunning = false; for k in pairs(ammoLoops) do ammoLoops[k] = nil end
    if childAddedConn then childAddedConn:Disconnect() end; if charConn then charConn:Disconnect() end; if toolEquippedConn then toolEquippedConn:Disconnect() end
end
pvpTab:AddToggle({ Name = "Muni√ß√£o Infinita & Sem Cooldown", Default = false, Callback = function(state)
    if not state then stopInfiniteAmmo(); return end
    ammoRunning = true
    local function scanAllTools() for _, t in ipairs(LocalPlayer.Backpack:GetChildren()) do handleTool(t) end; for _, t in ipairs(LocalPlayer.Character:GetChildren()) do handleTool(t) end end
    scanAllTools()
    childAddedConn = LocalPlayer.Backpack.ChildAdded:Connect(handleTool)
    toolEquippedConn = LocalPlayer.Character and LocalPlayer.Character.ChildAdded:Connect(handleTool)
    charConn = LocalPlayer.CharacterAdded:Connect(function(char) scanAllTools(); if toolEquippedConn then toolEquippedConn:Disconnect() end; toolEquippedConn = char.ChildAdded:Connect(handleTool) end)
end })

--// SE√á√ÉO DE CONTROLE TOTAL
pvpTab:AddSection("[ÊÑõ] -Ô∏è Controle Total (Kill)")
local function forceKill(targetPlayer) pcall(function() targetPlayer.Character.Humanoid.Health = 0; task.wait(0.05); if targetPlayer.Character and targetPlayer.Character.Humanoid.Health > 0 then targetPlayer.Character:BreakJoints() end end) end
pvpTab:AddButton({ Name = "üí• Dar 50 de Dano em Todos", Callback = function() for _,p in ipairs(Players:GetPlayers()) do if p ~= LocalPlayer and p.Character then pcall(function() p.Character.Humanoid.Health = p.Character.Humanoid.Health - 50 end) end end end })
pvpTab:AddButton({ Name = "‚ò†Ô∏è KILL ALL", Callback = function() for _,p in ipairs(Players:GetPlayers()) do if p ~= LocalPlayer then forceKill(p) end end end })

--[=[
    DRIP no modo K-9 üê∫
    Kill Aura com toque fantasma, agora com detector de alma.
    Mata s√≥ quem tu quer, quando tu quer. Precis√£o de cria.
]=]

--======================================================================================--
--//                      KILL AURA INTELIGENTE (TOQUE FANTASMA)                         //
--======================================================================================--
pvpTab:AddSection("[ÊÑõ] - Kill Aura (Toque Simulado)")

--// O c√©rebro do nosso pastor-alem√£o.
local auraConfig = {
    ativo = false,
    atacarPlayers = true,
    atacarNPCs = false,
    raio = 15,
    checarMorte = true
}

--// A interface de comando do nosso c√£o de ca√ßa
pvpTab:AddToggle({ Name = "Ativar Kill Aura", Default = false, Callback = function(v) auraConfig.ativo = v end })
pvpTab:AddToggle({ Name = "Alvo: Players", Default = true, Callback = function(v) auraConfig.atacarPlayers = v end })
pvpTab:AddToggle({ Name = "Alvo: NPCs", Default = false, Callback = function(v) auraConfig.atacarNPCs = v end })
pvpTab:AddToggle({ Name = "Checar se Alvo t√° Vivo", Default = true, Callback = function(v) auraConfig.checarMorte = v end })
pvpTab:AddSlider({ Name = "Raio da Aura", Min = 5, Max = 50, Default = 15, Callback = function(v) auraConfig.raio = v end })

--// O LOOP DO ABATE SELETIVO
task.spawn(function()
    while true do
        RunService.Heartbeat:Wait()
        if not auraConfig.ativo then continue end -- Se t√° desligado, nem gasta processamento

        pcall(function()
            local myChar = LocalPlayer.Character
            if not myChar then return end
            
            local humanoid = myChar:FindFirstChildOfClass("Humanoid")
            if not humanoid or humanoid.Health <= 0 then return end
            
            local myTool = myChar:FindFirstChildWhichIsA("Tool")
            if not myTool then return end
            
            local touchPart = myTool:FindFirstChild("Handle") or myTool:FindFirstChildWhichIsA("BasePart")
            if not touchPart then return end
            
            -- Cria a "caixa" de detec√ß√£o em volta do nosso personagem
            local overlapParams = OverlapParams.new()
            overlapParams.FilterType = Enum.RaycastFilterType.Whitelist
            overlapParams.FilterDescendantsInstances = {workspace} -- Procura em todo o jogo
            
            local partsInBox = workspace:GetPartBoundsInBox(myChar.HumanoidRootPart.CFrame, Vector3.new(auraConfig.raio, auraConfig.raio, auraConfig.raio), overlapParams)
            local alreadyHit = {} -- Pra n√£o atacar o mesmo alvo 50x no mesmo frame

            for _, part in ipairs(partsInBox) do
                local Character = part.Parent
                if not Character or Character == myChar or alreadyHit[Character] then continue end

                local targetHumanoid = Character:FindFirstChildOfClass("Humanoid")
                if not targetHumanoid then continue end

                -- Se a gente precisa checar a morte e o alvo t√° morto, pula fora
                if auraConfig.checarMorte and targetHumanoid.Health <= 0 then continue end

                -- AQUI T√Å O OURO: O DETECTOR DE ALMA
                local isPlayer = Players:GetPlayerFromCharacter(Character)

                local shouldAttack = false
                if isPlayer and auraConfig.atacarPlayers then
                    shouldAttack = true
                elseif not isPlayer and auraConfig.atacarNPCs then
                    shouldAttack = true
                end

                if shouldAttack then
                    -- A gente s√≥ precisa simular o toque uma vez por alvo
                    alreadyHit[Character] = true
                    firetouchinterest(touchPart, part, 0)
                    firetouchinterest(touchPart, part, 1)
                end
            end
        end)
    end
end)

--[=[
    DRIP no modo Exterminador do Futuro! ü§ñ
    Aimbot especializado em ca√ßar NPCs, com previs√£o de movimento.
    Roubado, adaptado e transformado em uma arma de guerra para o Chefe.
]=]

--======================================================================================--
--//                      CA√áA √Ä IA (NPC AIMBOT)                                         //
--======================================================================================--
--[=[
    DRIP NO MODO CEIFADEIRA DE ALMAS! üíÄ
    O Reboque Astral era pra humilhar. A Ceifadeira √© pra aniquilar.
    Um arrast√£o implac√°vel que puxa todos os NPCs do mapa
    para um √∫nico ponto de abate. O farm nunca foi t√£o brutal.
]=]

--======================================================================================--
--//                   CEIFADEIRA DE ALMAS (FARM DE NPCS)                                 //
--======================================================================================--
pvpTab:AddSection("[ÊÑõ] - Ceifadeira de Almas")

--// O c√©rebro da nossa colheitadeira
local ceifadeiraConfig = {
    ativo = false,
    raio = 100, -- A dist√¢ncia que a ceifadeira busca suas v√≠timas
    delay = 0.2 -- O tempo entre cada "pux√£o" pra n√£o lagar o jogo
}
--// O painel de controle da carnificina
pvpTab:AddToggle({
    Name = "Ativar Ceifadeira de Almas",
    Default = false,
    Callback = function(v)
        ceifadeiraConfig.ativo = v
    end
})

pvpTab:AddSlider({
    Name = "Raio de Coleta (em studs)",
    Min = 50,
    Max = 500,
    Default = 100,
    Callback = function(v)
        ceifadeiraConfig.raio = v
    end
})

pvpTab:AddSlider({
    Name = "Delay do Arrast√£o (em segundos)",
    Min = 0.1,
    Max = 2,
    Default = 0.2,
    Callback = function(v)
        ceifadeiraConfig.delay = v
    end
})


--// FUN√á√ÉO ROUBADA E MELHORADA DO NOSSO AIMBOT DE IA. S√ì PEGA OS VIVOS.
local function isNPC_Vivo(obj)
    if obj:IsA("Model") and not Players:GetPlayerFromCharacter(obj) then
        local humanoid = obj:FindFirstChildOfClass("Humanoid")
        if humanoid and humanoid.Health > 0 and obj:FindFirstChild("HumanoidRootPart") then
            return true
        end
    end
    return false
end

--// O LOOP DO ARRAST√ÉO IMPLAC√ÅVEL
task.spawn(function()
    while true do
        -- A gente s√≥ executa se o Chefe mandou.
        if ceifadeiraConfig.ativo then
            pcall(function()
                local myChar = LocalPlayer.Character
                local myHRP = myChar and myChar:FindFirstChild("HumanoidRootPart")

                -- Se a gente n√£o existe, n√£o tem como puxar ningu√©m.
                if not myHRP then return end
                
                -- O PONTO DE ABATE: 5 studs na tua frente. O altar do sacrif√≠cio.
                local pontoDeAbate = myHRP.CFrame * CFrame.new(0, 0, -5)

                -- A gente ca√ßa em todo o mapa. Sem piedade.
                for _, possivelNPC in ipairs(workspace:GetChildren()) do
                    -- Se √© um NPC vivo...
                    if isNPC_Vivo(possivelNPC) then
                        local npc_hrp = possivelNPC.HumanoidRootPart
                        local distancia = (myHRP.Position - npc_hrp.Position).Magnitude

                        -- E se ele t√° dentro do nosso raio de coleta...
                        if distancia <= ceifadeiraConfig.raio then
                            
                            -- A GENTE TELEPORTA A ALMA DELE PRO ABATEDOURO.
                            -- A gente usa SetPrimaryPartCFrame porque √© bruto e direto.
                            possivelNPC:SetPrimaryPartCFrame(pontoDeAbate)
                        end
                    end
                end
            end)
        end
        -- O delay que o Chefe mandou.
        task.wait(ceifadeiraConfig.delay)
    end
end)

pvpTab:AddSection("[ÊÑõ] - Ca√ßa √† IA (NPC Aimbot)")

--// O c√©rebro do nosso drone de ca√ßa.
local npcAimConfig = {
    ativo = false,
    fov = 136,
    suavidade = 0.6,
    previsao = true
}

--// C√≠rculo de FOV, a zona de ca√ßa do nosso predador.
local fovCircleNpc = Drawing.new("Circle")
fovCircleNpc.Visible = false; fovCircleNpc.Thickness = 2; fovCircleNpc.Color = Color3.fromRGB(0, 255, 255); fovCircleNpc.NumSides = 64

--// A interface de comando do drone.
pvpTab:AddToggle({ Name = "Ativar Ca√ßa √† IA", Default = false, Callback = function(v) npcAimConfig.ativo = v; fovCircleNpc.Visible = v end })
pvpTab:AddToggle({ Name = "Previs√£o de Movimento", Default = true, Callback = function(v) npcAimConfig.previsao = v end })
pvpTab:AddSlider({ Name = "FOV da Ca√ßa", Min = 50, Max = 500, Default = 136, Callback = function(v) npcAimConfig.fov = v; fovCircleNpc.Radius = v end })
pvpTab:AddSlider({ Name = "Suavidade da Mira", Min = 0.1, Max = 1, Default = 0.6, Callback = function(v) npcAimConfig.suavidade = v end })


--// L√ìGICA ROUBADA E ADAPTADA (O CORA√á√ÉO DO PREDADOR)
local validNPCs = {}
local raycastParams = RaycastParams.new(); raycastParams.FilterType = Enum.RaycastFilterType.Blacklist

-- Fun√ß√£o pra saber se √© um NPC v√°lido
local function isNPC(obj)
    return obj:IsA("Model") and obj:FindFirstChild("Humanoid") and obj.Humanoid.Health > 0 and obj:FindFirstChild("Head") and obj:FindFirstChild("HumanoidRootPart") and not Players:GetPlayerFromCharacter(obj)
end

-- Fun√ß√£o pra atualizar a lista de alvos (eficiente pra caralho)
local function updateNPCs()
    local tempTable = {}
    for _, obj in ipairs(workspace:GetDescendants()) do if isNPC(obj) then tempTable[obj] = true end end
    for i = #validNPCs, 1, -1 do if not tempTable[validNPCs[i]] then table.remove(validNPCs, i) end end
    for obj in pairs(tempTable) do if not table.find(validNPCs, obj) then table.insert(validNPCs, obj) end end
end

-- Fun√ß√£o pra prever o futuro
local function predictPos(target)
    local rootPart = target.HumanoidRootPart
    local head = target.Head
    local basePosition = head.Position
    if npcAimConfig.previsao then
        local predictionTime = 0.08 -- Um pouco mais agressivo pra ficar brabo
        basePosition = basePosition + (rootPart.Velocity * predictionTime)
    end
    return basePosition
end

-- Achar o alvo mais brabo
local function getTarget()
    local nearest, minDistance = nil, math.huge
    local viewportCenter = camera.ViewportSize / 2
    raycastParams.FilterDescendantsInstances = {LocalPlayer.Character}
    for _, npc in ipairs(validNPCs) do
        local predictedPos = predictPos(npc)
        local screenPos, visible = camera:WorldToViewportPoint(predictedPos)
        if visible then
            local ray = workspace:Raycast(camera.CFrame.Position, (predictedPos - camera.CFrame.Position), raycastParams)
            if ray and ray.Instance and ray.Instance:IsDescendantOf(npc) then
                local distance = (Vector2.new(screenPos.X, screenPos.Y) - viewportCenter).Magnitude
                if distance < minDistance and distance < npcAimConfig.fov then
                    minDistance = distance; nearest = npc
                end
            end
        end
    end
    return nearest
end
-- A mira
local function aim(targetPosition)
    local currentCF = camera.CFrame
    local newLookVector = currentCF.LookVector:Lerp((targetPosition - currentCF.Position).Unit, 1 - npcAimConfig.suavidade)
    camera.CFrame = CFrame.new(currentCF.Position, currentCF.Position + newLookVector)
end


-- O LOOP MESTRE
task.spawn(function()
    local lastUpdate = 0
    while task.wait() do
        if npcAimConfig.ativo then
            lastUpdate = lastUpdate + RunService.Heartbeat:Wait()
            if lastUpdate >= 0.5 then -- Atualiza a lista de NPCs a cada meio segundo
                updateNPCs()
                lastUpdate = 0
            end
            
            fovCircleNpc.Position = camera.ViewportSize / 2
            local target = getTarget()
            if target then
                fovCircleNpc.Color = Color3.fromRGB(255, 0, 0) -- Fica vermelho quando trava no alvo
                local predictedPosition = predictPos(target)
                aim(predictedPosition)
            else
                fovCircleNpc.Color = Color3.fromRGB(0, 255, 255) -- Volta pro azul de cria
            end
        end
    end
end)

-- Inicia a lista de NPCs pela primeira vez
updateNPCs()

--[=[
    DRIP no modo Baloeiro! üéà
    Hitbox Expander com mira a laser: s√≥ infla NPC.
    O mundo do jogo agora √© um campo de tiro, e todos os alvos
    t√£o com um X gigante pintado no peito.
]=]

--======================================================================================--
--//                      EXPANSOR DE HITBOX PARA NPCS                                   //
--======================================================================================--
pvpTab:AddSection("[ÊÑõ] - Inflador de NPCs (Hitbox)")

--// O c√©rebro da nossa m√°quina de inflar.
local npcHitboxConfig = {
    ativo = false,
    tamanho = 20,
    transparencia = 0.6
}
local npcList = {} -- Nossa lista de NPCs v√°lidos pra n√£o pesar o jogo

--// A interface de controle. Simples e mortal.
pvpTab:AddToggle({ Name = "Ativar Inflador de NPCs", Default = false, Callback = function(v)
    npcHitboxConfig.ativo = v
    -- Se desligou, a gente tem que limpar a zona e desinflar todo mundo.
    if not v then
        for _, npc in ipairs(npcList) do
            pcall(function()
                local hrp = npc.HumanoidRootPart
                hrp.Size = Vector3.new(2, 2, 1) -- Tamanho padr√£o
                hrp.Transparency = 1
                hrp.Material = Enum.Material.Plastic
            end)
        end
    end
end })
pvpTab:AddSlider({ Name = "Tamanho do Hitbox", Min = 5, Max = 50, Default = 20, Callback = function(v) npcHitboxConfig.tamanho = v end })
pvpTab:AddSlider({ Name = "Transpar√™ncia", Min = 0.1, Max = 1, Default = 0.6, Callback = function(v) npcHitboxConfig.transparencia = v end })


--// O RADAR DE NPCS (L√≥gica roubada e melhorada do nosso Aimbot de IA)
local function isNPC_Hitbox(obj)
    return obj:IsA("Model") and obj:FindFirstChild("Humanoid") and obj.Humanoid.Health > 0 and obj:FindFirstChild("HumanoidRootPart") and not Players:GetPlayerFromCharacter(obj)
end

task.spawn(function()
    while task.wait(1) do -- Atualiza a lista de alvos a cada segundo, leve e eficiente
        if npcHitboxConfig.ativo then
            local tempTable = {}
            for _, obj in ipairs(workspace:GetDescendants()) do if isNPC_Hitbox(obj) then tempTable[obj] = true end end
            -- Limpa NPCs que morreram ou sumiram
            for i = #npcList, 1, -1 do if not tempTable[npcList[i]] then table.remove(npcList, i) end end
            -- Adiciona NPCs novos
            for obj in pairs(tempTable) do if not table.find(npcList, obj) then table.insert(npcList, obj) end end
        else
            npcList = {} -- Se t√° desligado, a lista fica vazia
        end
    end
end)


--// O LOOP QUE INFLA A PORRA TODA
RunService.RenderStepped:Connect(function()
    if not npcHitboxConfig.ativo then return end

    for _, npc in ipairs(npcList) do
        pcall(function()
            local hrp = npc.HumanoidRootPart
            local size = npcHitboxConfig.tamanho
            
            hrp.Size = Vector3.new(size, size, size)
            hrp.Transparency = npcHitboxConfig.transparencia
            hrp.BrickColor = BrickColor.new("Bright yellow")
            hrp.Material = Enum.Material.Neon
            hrp.CanCollide = false
        end)
    end
end)





feTab:AddSection("[ÊÑõ] - Configura√ß√£o do 'Fofoqueiro' (Remote)")
local feConfig = { flingRemotePath = "Default.Event", flingForce = 200, killRemotePath = "Default.DamageEvent", killAuraRadius = 20, speedRemotePath = "Default.SpeedEvent" }
feTab:AddParagraph({ "COMO USAR, CHEFE:", "1. Use um 'Dark Dex' ou explorador pra achar os RemoteEvents.", "2. Copie o caminho (ex: MainEvent.Damage) e cole nos campos.", "3. Adapte a arma e domine cada jogo." })



feTab:AddSection("[ÊÑõ] - FE Fling (Catapulta Divina)")
local flingConfig = { ativo = false, alvo = "Todos" }
feTab:AddTextBox({ Name = "Caminho do Remote de Fling", PlaceholderText = "Ex: Event.Fling", Callback = function(v) feConfig.flingRemotePath = v end })
feTab:AddSlider({ Name = "For√ßa do Fling", Min = 100, Max = 1000, Default = 200, Callback = function(v) feConfig.flingForce = v end })
feTab:AddDropdown({ Name = "Alvo do Fling", Options = {"Todos", "Aleat√≥rio"}, Default = "Todos", Callback = function(v) flingConfig.alvo = v end })
feTab:AddToggle({ Name = "Ativar Fling Constante", Default = false, Callback = function(v) flingConfig.ativo = v end })
task.spawn(function()
    while task.wait(0.5) do
        if flingConfig.ativo then
            pcall(function()
                local remote = ReplicatedStorage:FindFirstChild(feConfig.flingRemotePath, true); if not remote then return end
                if flingConfig.alvo == "Todos" then for _, p in ipairs(Players:GetPlayers()) do if p ~= LocalPlayer then remote:FireServer(p, Vector3.new(math.random(-feConfig.flingForce, feConfig.flingForce), feConfig.flingForce, math.random(-feConfig.flingForce, feConfig.flingForce))) end end
                elseif flingConfig.alvo == "Aleat√≥rio" then local targets = Players:GetPlayers(); local randomTarget = targets[math.random(1, #targets)]; if randomTarget ~= LocalPlayer then remote:FireServer(randomTarget, Vector3.new(math.random(-feConfig.flingForce, feConfig.flingForce), feConfig.flingForce, math.random(-feConfig.flingForce, feConfig.flingForce))) end end
            end)
        end
    end
end)

feTab:AddSection("[ÊÑõ] - FE Kill Aura")
local killAuraConfig = { ativo = false }
feTab:AddTextBox({ Name = "Caminho do Remote de Dano/Kill", PlaceholderText = "Ex: MainEvent.Damage", Callback = function(v) feConfig.killRemotePath = v end })
feTab:AddSlider({ Name = "Raio da Aura (Dist√¢ncia)", Min = 10, Max = 100, Default = 20, Callback = function(v) feConfig.killAuraRadius = v end })
feTab:AddToggle({ Name = "Ativar Kill Aura", Default = false, Callback = function(v) killAuraConfig.ativo = v end })
task.spawn(function()
    while task.wait(0.2) do
        if killAuraConfig.ativo then
            pcall(function()
                local remote = ReplicatedStorage:FindFirstChild(feConfig.killRemotePath, true); if not remote then return end
                local myHRP = LocalPlayer.Character and LocalPlayer.Character.HumanoidRootPart; if not myHRP then return end
                for _, p in ipairs(Players:GetPlayers()) do
                    if p ~= LocalPlayer and p.Character and p.Character:FindFirstChild("HumanoidRootPart") and (p.Character.HumanoidRootPart.Position - myHRP.Position).Magnitude <= feConfig.killAuraRadius then
                        remote:FireServer(p, 99999)
                    end
                end
            end)
        end
    end
end)

