    campoDeForcaAtivo = state
    local ff = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("ForceField")
    if state and not ff and LocalPlayer.Character then Instance.new("ForceField", LocalPlayer.Character)
    elseif ff then ff.Visible = state end
end })
