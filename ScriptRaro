--======================================================================================--
--//                      CA√áADOR DE ALVOS (TELEPORTE INTELIGENTE)                       //
--======================================================================================--
teleportTab:AddSection("[ÊÑõ] - Teleportar Para Jogador")

--// O c√©rebro do nosso farejador.
local teleportTargetName = ""

--// A interface de ca√ßa. Simples e mortal.
teleportTab:AddTextBox({
    Name = "Nome ou In√≠cio do Nick do Alvo",
    PlaceholderText = "Digite 'rar' pra achar 'Raro_Modz'...",
    Callback = function(v)
        teleportTargetName = v
    end
})

teleportTab:AddButton({
    Name = "TELEPORTAR PARA O ALVO",
    Callback = function()
        pcall(function()
            local alvoParcial = teleportTargetName:lower() -- Deixa tudo min√∫sculo pra n√£o ter erro
            if alvoParcial == "" then return end

            local alvoEncontrado = nil

            -- O loop da ca√ßada
            for _, player in ipairs(Players:GetPlayers()) do
                if player ~= LocalPlayer then
                    local nomeCompleto = player.Name:lower()
                    
                    -- A m√°gica acontece aqui: checa se o nome do player COME√áA com o que tu digitou
                    if nomeCompleto:sub(1, #alvoParcial) == alvoParcial then
                        alvoEncontrado = player
                        break -- Achou o primeiro? J√° era, para de procurar.
                    end
                end
            end

            -- Se a ca√ßada teve sucesso, a gente executa o abate (o teleporte)
            if alvoEncontrado and alvoEncontrado.Character and alvoEncontrado.Character:FindFirstChild("HumanoidRootPart") then
                local myHRP = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                if not myHRP then return end
                
                local targetHRP = alvoEncontrado.Character.HumanoidRootPart
                myHRP.CFrame = targetHRP.CFrame * CFrame.new(0, 5, 0) -- Joga pra cima da cabe√ßa pra n√£o bugar
                print("DRIP: Alvo '" .. alvoEncontrado.Name .. "' encontrado. Teleporte efetuado.")
            else
                warn("DRIP: Nenhum alvo encontrado com o in√≠cio '" .. teleportTargetName .. "'.")
            end
        end)
    end
})

--[=[
    DRIP no modo Doutor Estranho! üåÄ
    Sistema de Checkpoint pessoal. Salve sua posi√ß√£o, desafie a morte.
    O mapa √© teu, as regras de spawn s√£o tuas.
]=]

--[=[
    DRIP no modo Operador de Metr√¥! üöá
    Sistema de M√∫ltiplos Checkpoints. O mapa √© teu tabuleiro.
    Crie sua rede de bases secretas.
]=]
--[=[
    DRIP no modo General! üéñÔ∏è
    Controle o teu ponto de retorno. Decida as regras da tua
    pr√≥pria imortalidade. O campo de batalha √© teu.
]=]

--======================================================================================--
--//                      √ÇNCORA T√ÅTICA (SAVE/CLEAR SPAWNPOINT)                          //
--======================================================================================--
teleportTab:AddSection("[ÊÑõ] -  Salvar Spawn")

--// O c√©rebro da nossa opera√ß√£o. A gente s√≥ precisa saber onde t√° nossa √¢ncora.
local ancoraTatica = nil

--// O bot√£o que crava a tua bandeira no ch√£o.
teleportTab:AddButton({
    Name = "SALVAR SPAWNPOINT ATUAL",
    Callback = function()
        pcall(function()
            local myHRP = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
            if not myHRP then
                warn("DRIP: N√£o foi poss√≠vel salvar. Personagem n√£o encontrado.")
                return
            end

            -- Se a gente j√° tinha uma √¢ncora, destr√≥i a antiga primeiro.
            if ancoraTatica then
                ancoraTatica:Destroy()
            end

            -- Cria a nossa nova √¢ncora (uma Part invis√≠vel e indestrut√≠vel)
            ancoraTatica = Instance.new("Part", workspace)
            ancoraTatica.Name = "DRIP_AncoraTatica_" .. LocalPlayer.Name
            ancoraTatica.Size = Vector3.new(4, 1, 4)
            ancoraTatica.Anchored = true
            ancoraTatica.CanCollide = false
            ancoraTatica.Transparency = 1
            ancoraTatica.CFrame = myHRP.CFrame

            -- A M√ÅGICA: A gente aponta a "plaquinha" de respawn pra nossa √¢ncora.
            LocalPlayer.RespawnLocation = ancoraTatica

            print("DRIP: √Çncora T√°tica cravada! Voc√™ renascer√° aqui.")
        end)
    end
})

--// O bot√£o que apaga nossos rastros.
teleportTab:AddButton({
    Name = "LIMPAR SPAWNPOINT",
    Callback = function()
        pcall(function()
            if ancoraTatica then
                ancoraTatica:Destroy()
                ancoraTatica = nil
                -- Limpa a "plaquinha", devolvendo o controle pro jogo.
                LocalPlayer.RespawnLocation = nil
                print("DRIP: √Çncora T√°tica removida. Spawnpoint resetado para o padr√£o.")
            else
                warn("DRIP: Nenhuma √Çncora T√°tica para limpar.")
            end
        end)
    end
})

teleportTab:AddParagraph({
    "Instru√ß√µes:",
    "1. V√° para o local onde voc√™ quer renascer.",
    "2. Clique em 'SALVAR SPAWNPOINT ATUAL'.",
    "3. Agora, toda vez que voc√™ morrer, voltar√° para esse ponto.",
    "4. Clique em 'LIMPAR' para voltar a renascer nos spawns normais."
})
--======================================================================================--
--//                      REDE DE CHECKPOINTS (M√∫ltiplas Posi√ß√µes)                       //
--======================================================================================--
teleportTab:AddSection("[ÊÑõ] - Rede de Bases Secretas")

--// O nosso "√°lbum de fotos". Uma tabela pra guardar at√© 5 posi√ß√µes.
local savedPositions = {}

--// A gente vai criar os bot√µes num loop. √â mais limpo e mais brabo.
for i = 1, 5 do
    -- O texto que vai aparecer na UI pra cada posi√ß√£o.
    local positionDisplay = teleportTab:AddParagraph({ "Posi√ß√£o " .. i .. " Salva:", "Nenhuma" })

    -- O bot√£o de SALVAR
    teleportTab:AddButton({
        Name = "Salvar Posi√ß√£o " .. i,
        Callback = function()
            pcall(function()
                local myHRP = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                if myHRP then
                    savedPositions[i] = myHRP.CFrame
                    local pos = savedPositions[i].Position
                    -- Atualiza a UI pra gente saber que funcionou
                    positionDisplay:SetText({"Posi√ß√£o " .. i .. " Salva:", string.format("X: %.1f, Y: %.1f, Z: %.1f", pos.X, pos.Y, pos.Z)})
                    print("DRIP: Posi√ß√£o " .. i .. " salva!")
                else
                    warn("DRIP: N√£o foi poss√≠vel salvar. Personagem n√£o encontrado.")
                end
            end)
        end
    })

    -- O bot√£o de TELEPORTAR
    teleportTab:AddButton({
        Name = "Teleportar para Posi√ß√£o " .. i,
        Callback = function()
            pcall(function()
                if savedPositions[i] then
                    local myHRP = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                    if myHRP then
                        myHRP.CFrame = savedPositions[i]
                        print("DRIP: Teleportado para a posi√ß√£o " .. i .. "!")
                    end
                else
                    warn("DRIP: Posi√ß√£o " .. i .. " n√£o foi salva ainda.")
                end
            end)
        end
    })
    
    -- Uma linha pra separar cada conjunto de bot√µes e deixar o bagulho organizado.
    if i < 5 then
        teleportTab:AddParagraph({" ", " "}) -- Gambiarra de cria pra dar espa√ßo
    end
end



--======================================================================================--
--//                             3. RECHEANDO A ABA 'GAME'                              //
--======================================================================================--
--[=[
    DRIP NO MODO CJ, CARALHO!  Grove Street 4 Life! üíö
    Oficina de Handling. A gente n√£o s√≥ dirige, a gente CRIA a porra da f√≠sica.
    Transforme qualquer carro numa lenda do asfalto.
]=]

--======================================================================================--
--//                      OFICINA DE HANDLING (MEC√ÇNICO FANTASMA)                        //
--======================================================================================--
gameTab:AddSection("[ÊÑõ] - Oficina de Handling")

--// O c√©rebro do nosso engenheiro. Aqui a gente guarda a nossa receita.
local handlingConfig = {
    ativo = false,
    torque = 5000,
    velocidade = 200,
    densidade = 0.7, -- Padr√£o do Roblox
    atrito = 0.8, -- Mais alto = mais grip
    forcaSuspensao = 10000 -- Mais alto = mais duro
}

--// O painel de controle da nossa oficina.
gameTab:AddToggle({ Name = "Ativar Mec√¢nico Fantasma", Default = false, Callback = function(v) handlingConfig.ativo = v end })
gameTab:AddSlider({ Name = "For√ßa do Motor (Torque)", Min = 1000, Max = 50000, Default = 5000, Callback = function(v) handlingConfig.torque = v end })
gameTab:AddSlider({ Name = "Velocidade M√°xima", Min = 50, Max = 500, Default = 200, Callback = function(v) handlingConfig.velocidade = v end })
gameTab:AddSlider({ Name = "Peso/Densidade do Carro", Min = 0.1, Max = 5, Default = 0.7, Callback = function(v) handlingConfig.densidade = v end })
gameTab:AddSlider({ Name = "Ader√™ncia dos Pneus (Atrito)", Min = 0.1, Max = 2, Default = 0.8, Callback = function(v) handlingConfig.atrito = v end })
gameTab:AddSlider({ Name = "Dureza da Suspens√£o", Min = 1000, Max = 50000, Default = 10000, Callback = function(v) handlingConfig.forcaSuspensao = v end })

--// O LOOP MESTRE. O MEC√ÇNICO QUE NUNCA DORME.
task.spawn(function()
    local carroAtual = nil
    while RunService.Heartbeat:Wait() do
        pcall(function()
            local humanoid = LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
            if not humanoid then return end
            
            local seatPart = humanoid.SeatPart
            
            -- Se o script t√° desligado OU a gente saiu do carro, restaura a porra toda
            if not handlingConfig.ativo or not seatPart or not seatPart:IsA("VehicleSeat") then
                if carroAtual then
                    for _, part in ipairs(carroAtual:GetDescendants()) do
                        if part:IsA("BasePart") then part.CustomPhysicalProperties = nil end
                    end
                    carroAtual = nil
                end
                return
            end
            
            -- Se a gente acabou de entrar num carro novo
            if seatPart and seatPart.Parent ~= carroAtual then
                carroAtual = seatPart.Parent
            end
            
            -- APLICA A NOSSA RECEITA DE CRIA
            seatPart.MaxSpeed = handlingConfig.velocidade
            seatPart.Torque = handlingConfig.torque
            
            local propriedadesCustom = PhysicalProperties.new(handlingConfig.densidade, handlingConfig.atrito, 0.5, 1, 1)

            for _, part in ipairs(carroAtual:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CustomPhysicalProperties = propriedadesCustom
                end
                if part:IsA("SpringConstraint") then
                    part.Stiffness = handlingConfig.forcaSuspensao
                end
            end
        end)
    end
end)
--======================================================================================--
--//                   BISTURI INTELIGENTE (ACHAR A ALMA DO CARRO)                      //
--======================================================================================--
local function encontrarAlmaDoCarro(veiculo)
    local alma, maiorMassa = nil, 0
    -- A gente vai ca√ßar cada pe√ßa dentro do modelo do carro
    for _, peca in ipairs(veiculo:GetDescendants()) do
        -- Se a pe√ßa √© uma parte f√≠sica e tem uma massa maior que a √∫ltima que a gente achou...
        if peca:IsA("BasePart") and peca:GetMass() > maiorMassa then
            maiorMassa = peca:GetMass() -- A gente atualiza a maior massa encontrada
            alma = peca                -- E essa pe√ßa vira a nossa nova "alma"
        end
    end
    -- Se n√£o achou porra nenhuma, a gente devolve o banco mesmo, s√≥ pra n√£o dar erro.
    return alma or veiculo:FindFirstChildWhichIsA("VehicleSeat")
end
