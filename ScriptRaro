task.spawn(function()
    while task.wait(1) do
        if regenAtivo then
            pcall(function()
                local hum = LocalPlayer.Character.Humanoid
                if hum.Health < hum.MaxHealth then hum.Health = hum.Health + 5 end
            end)
        end
    end
end)



--// SE√á√ÉO DE GODMODES
playerTab:AddSection("[ÊÑõ] -Ô∏è Modos de Deus")
local godModeLoop, godModeAtivo = nil, false
playerTab:AddDropdown({ Name = "Escolha seu GodMode", Options = {"Desativado", "GodMode Cl√°ssico", "Regen Absurdo"}, Default = "Desativado", Callback = function(opcao)
    godModeAtivo = false
    if godModeLoop then godModeLoop:Disconnect(); godModeLoop = nil end
    pcall(function() local h = LocalPlayer.Character.Humanoid; h.MaxHealth = 100; task.wait(); h.Health = 100 end)
    if opcao == "GodMode Cl√°ssico" then pcall(function() local h = LocalPlayer.Character.Humanoid; h.MaxHealth = math.huge; task.wait(); h.Health = math.huge end)
    elseif opcao == "Regen Absurdo" then
        godModeAtivo = true
        godModeLoop = RunService.Heartbeat:Connect(function()
            if not godModeAtivo then godModeLoop:Disconnect(); godModeLoop = nil; return end
            pcall(function() LocalPlayer.Character.Humanoid.Health = LocalPlayer.Character.Humanoid.MaxHealth end)
        end)
    end
end })
local healthRunning, healthCharConn, healthChangedConn, diedConn = false, nil, nil, nil
playerTab:AddToggle({ Name = "GodMode Reativo (Anti-Dano)", Default = false, Callback = function(state)
    healthRunning = state
    if not state then
        if healthChangedConn then healthChangedConn:Disconnect() end; if diedConn then diedConn:Disconnect() end; if healthCharConn then healthCharConn:Disconnect() end
        return
    end
    local function connectChar(char)
        if healthChangedConn then healthChangedConn:Disconnect() end; if diedConn then diedConn:Disconnect() end
        local hum = char:WaitForChild("Humanoid", 5)
        if hum then
            healthChangedConn = hum.HealthChanged:Connect(function(newHealth) if healthRunning and newHealth < hum.MaxHealth then hum.Health = hum.MaxHealth end end)
            diedConn = hum.Died:Connect(function() if healthRunning then task.wait(0.1); pcall(function() hum.Health = hum.MaxHealth end) end end)
            hum.Health = hum.MaxHealth
        end
    end
    if LocalPlayer.Character then connectChar(LocalPlayer.Character) end
    healthCharConn = LocalPlayer.CharacterAdded:Connect(connectChar)
end })

--// SE√á√ÉO DE MOBILIDADE AVAN√áADA
playerTab:AddSection("[ÊÑõ] - Mobilidade de Ninja")
local dashCooldown, bhopAtivo = false, false
playerTab:AddButton({ Name = "Dash (Tecla Q)", Callback = function() end })
UserInputService.InputBegan:Connect(function(input, gpe)
    if gpe then return end
    if input.KeyCode == Enum.KeyCode.Q and not dashCooldown then
        dashCooldown = true
        pcall(function()
            local myHRP = LocalPlayer.Character.HumanoidRootPart
            local dashForce = Instance.new("BodyVelocity", myHRP); dashForce.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
            dashForce.Velocity = myHRP.CFrame.LookVector * 100; Debris:AddItem(dashForce, 0.2)
        end)
        task.wait(1); dashCooldown = false
    end
end)
playerTab:AddToggle({ Name = "Bunny Hop Autom√°tico", Default = false, Callback = function(state) bhopAtivo = state end })
task.spawn(function()
    while task.wait() do
        if bhopAtivo then pcall(function() if LocalPlayer.Character.Humanoid:GetState() == Enum.HumanoidStateType.Landed then LocalPlayer.Character.Humanoid.Jump = true end end) end
    end
end)

