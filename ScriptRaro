trollTab:AddSection("[ÊÑõ] - Bomba de Confus√£o")
local bombaConfig = { ativo = false, alvo = nil, bomba = nil }
local bombaDropdown = trollTab:AddDropdown({ Name = "SELECIONAR ALVO DA BOMBA", Options = {}, Callback = function(v) bombaConfig.alvo = Players:FindFirstChild(v) end })
trollTab:AddButton({ Name = "ATUALIZAR LISTA", Callback = function() local l={}; for _,p in ipairs(Players:GetPlayers()) do if p ~= LocalPlayer then table.insert(l, p.Name) end end; pcall(function() bombaDropdown:SetOptions(l) end) end })
trollTab:AddToggle({ Name = "Jogar Bomba de Confus√£o", Default = false, Callback = function(state)
    bombaConfig.ativo = state
    if not state and bombaConfig.bomba then bombaConfig.bomba:Destroy(); bombaConfig.bomba = nil end
end })
RunService.RenderStepped:Connect(function()
    if bombaConfig.ativo and bombaConfig.alvo and bombaConfig.alvo.Character and bombaConfig.alvo.Character:FindFirstChild("HumanoidRootPart") then
        local targetHRP = bombaConfig.alvo.Character.HumanoidRootPart
        if not bombaConfig.bomba then
            bombaConfig.bomba = Instance.new("Model", workspace)
            for i = 1, 5 do local part = Instance.new("Part", bombaConfig.bomba); part.Size = Vector3.new(15,15,15); part.Anchored = true; part.CanCollide = false; part.Material = Enum.Material.Neon; part.Color = Color3.fromHSV(math.random(), 1, 1); part.Transparency = 0.6 end
        end
        bombaConfig.bomba:SetPrimaryPartCFrame(targetHRP.CFrame * CFrame.Angles(tick()*2, tick()*3, tick()*4))
        for _, part in ipairs(bombaConfig.bomba:GetChildren()) do if part:IsA("Part") then part.Color = Color3.fromHSV((tick() * 0.2) % 1, 1, 1) end end
    elseif bombaConfig.bomba then bombaConfig.bomba:Destroy(); bombaConfig.bomba = nil end
end)
