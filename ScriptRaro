gameTab:AddSection("[ÊÑõ] - Aspirador de Dinheiro T√°tico")

--// O c√©rebro do nosso mestre de assalto.
local farmConfig = {
    ativo = false,
    espAtivo = true,
    delay = 1, -- Agora em segundos
    modoBatedor = false, -- O nosso novo brinquedo
    palavrasChave = {"cash", "money", "dinheiro", "coin", "dollar", "gold", "nota", "banknote"},
    
    cacheDeDinheiro = {},
    ultimoCacheUpdate = 0
}

local espDrawingsDinheiro = {}

--// A interface de controle do cofre, agora com mais poder.
gameTab:AddToggle({ Name = "Ativar Aspirador de Dinheiro", Default = false, Callback = function(v) farmConfig.ativo = v end })
gameTab:AddToggle({ Name = "Ativar ESP de Dinheiro", Default = true, Callback = function(v)
    farmConfig.espAtivo = v
    if not v then for _, d in pairs(espDrawingsDinheiro) do d:Remove() end; espDrawingsDinheiro = {} end
end })
gameTab:AddToggle({ Name = "Modo Batedor (S√≥ Teleporte R√°pido)", Default = false, Callback = function(v) farmConfig.modoBatedor = v end })
gameTab:AddSlider({ Name = "Delay da Coleta (Segundos)", Min = 1, Max = 5, Default = 1, Callback = function(v) farmConfig.delay = v end })


--// O RADAR INTELIGENTE (continua o mesmo, porque √© brabo)
local function atualizarCacheDeDinheiro()
    local myRoot = LocalPlayer.Character and LocalPlayer.Character.HumanoidRootPart
    if not myRoot then return end
    
    local dinheiroNoMapa = {}
    for _, v in ipairs(Workspace:GetDescendants()) do
        if v:IsA("BasePart") and v.Parent then
            local nome = v.Name:lower()
            for _, keyword in ipairs(farmConfig.palavrasChave) do
                if nome:find(keyword) then
                    table.insert(dinheiroNoMapa, v); break
                end
            end
        end
    end
    
    table.sort(dinheiroNoMapa, function(a, b) return (myRoot.Position - a.Position).Magnitude < (myRoot.Position - b.Position).Magnitude end)
    farmConfig.cacheDeDinheiro = dinheiroNoMapa
end

-- O LOOP MESTRE (O ASPIRADOR, AGORA COM DUAS VELOCIDADES)
task.spawn(function()
    while true do
        -- A M√ÅGICA ACONTECE AQUI, CHEFE:
        -- Se o modo batedor t√° ligado, o delay √© quase zero. Se n√£o, ele usa o slider.
        local delayAtual = farmConfig.modoBatedor and 0.01 or farmConfig.delay
        task.wait(delayAtual)

        pcall(function()
            if not farmConfig.ativo or not LocalPlayer.Character then return end
            
            if tick() - farmConfig.ultimoCacheUpdate > 5 then
                atualizarCacheDeDinheiro()
                farmConfig.ultimoCacheUpdate = tick()
            end
            
            if #farmConfig.cacheDeDinheiro > 0 then
                local myRoot = LocalPlayer.Character.HumanoidRootPart
                local alvo = farmConfig.cacheDeDinheiro[1]
                
                if myRoot and alvo and alvo.Parent then
                    myRoot.CFrame = alvo.CFrame + Vector3.new(0, 3, 0)
                    table.remove(farmConfig.cacheDeDinheiro, 1)
                end
            end
        end)
    end
end)

-- O LOOP DO ESP (O RADAR DO TIO PATINHAS)
RunService.RenderStepped:Connect(function()
    if not farmConfig.espAtivo then return end

    for item, drawing in pairs(espDrawingsDinheiro) do
        if not item.Parent then drawing:Remove(); espDrawingsDinheiro[item] = nil end
    end
    
    for _, item in ipairs(farmConfig.cacheDeDinheiro) do
        if not espDrawingsDinheiro[item] then
            local text = Drawing.new("Text"); text.Text = item.Name; text.Color = Color3.fromRGB(50, 255, 50); text.Size = 18; text.Center = true; text.Outline = true
            espDrawingsDinheiro[item] = text
        end
        local pos, inView = camera:WorldToViewportPoint(item.Position)
        local drawing = espDrawingsDinheiro[item]
        drawing.Visible = inView
        if inView then drawing.Position = Vector2.new(pos.X, pos.Y) end
    end
end)


--[=[
    DRIP NO MODO FANTASMA DE ASFALTO! üëªüèéÔ∏è
    Foda-se as paredes. Foda-se a f√≠sica. O caminho √© reto.
    Atravesse tudo, domine todos os atalhos.
]=]

--======================================================================================--
--//                      GARAGEM FANTASMA (CAR NOCLIP)                                  //
--======================================================================================--
gameTab:AddSection("üëª Garagem Fantasma (Car Noclip)")

--// O c√©rebro da nossa opera√ß√£o fantasma.
local noclipCarroConfig = {
    ativo = false,
    carroAtual = nil,
    pecasOriginais = {} -- A gente guarda as propriedades originais pra n√£o dar merda
}

--// O Toggle que ativa o modo fantasma.
gameTab:AddToggle({
    Name = "Ativar Noclip de Carro",
    Default = false,
    Callback = function(v)
        noclipCarroConfig.ativo = v
        
        -- Se desligou, a gente restaura a porra toda ao normal.
        if not v and noclipCarroConfig.carroAtual and next(noclipCarroConfig.pecasOriginais) then
            for peca, podeColidir in pairs(noclipCarroConfig.pecasOriginais) do
                pcall(function()
                    peca.CanCollide = podeColidir
                end)
            end
            noclipCarroConfig.carroAtual = nil
            noclipCarroConfig.pecasOriginais = {}
        end
    end
})

--// O LOOP MESTRE QUE VIRA FANTASMA
task.spawn(function()
    while RunService.Heartbeat:Wait() do
        pcall(function()
            local humanoid = LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
            if not humanoid then return end
            
            local seatPart = humanoid.SeatPart
            
            -- Se o Noclip t√° ligado E a gente t√° num carro...
            if noclipCarroConfig.ativo and seatPart and seatPart:IsA("VehicleSeat") then
                local carro = seatPart:FindFirstAncestorOfClass("Model")
                if not carro then return end

                -- Se √© um carro novo, a gente salva as configs originais antes de foder tudo
                if carro ~= noclipCarroConfig.carroAtual then
                    noclipCarroConfig.carroAtual = carro
                    noclipCarroConfig.pecasOriginais = {}
                    for _, peca in ipairs(carro:GetDescendants()) do
                        if peca:IsA("BasePart") then
                            noclipCarroConfig.pecasOriginais[peca] = peca.CanCollide
                        end
                    end
                end

                -- A M√ÅGICA NEGRA: Deixa a porra toda sem colis√£o
                for peca in pairs(noclipCarroConfig.pecasOriginais) do
                    if peca and peca.Parent then
                        peca.CanCollide = false
                    end
                end

            -- Se a gente saiu do carro, restaura o √∫ltimo carro que a gente tava
            elseif noclipCarroConfig.carroAtual then
                for peca, podeColidir in pairs(noclipCarroConfig.pecasOriginais) do
                    if peca and peca.Parent then
                        peca.CanCollide = podeColidir
                    end
                end
                noclipCarroConfig.carroAtual = nil
                noclipCarroConfig.pecasOriginais = {}
            end
        end)
    end
end)

gameTab:AddSection("[ÊÑõ] - Ca√ßa At√¥mico (Rei dos C√©us)")

--// O c√©rebro do nosso piloto. Agora com rota√ß√£o.
local kingHunter = {
    ativo = false,
    potenciaHorizontal = 150,
    potenciaVertical = 100,
    velocidadeRotacao = 5,
    
    velocidadeHorizontal = 0,
    velocidadeVertical = 0,
    
    acelerando, freando, subindo, descendo, virandoEsq, virandoDir = false, false, false, false, false, false,
    
    estabilizador = nil,
    motorRotacao = nil, -- O nosso novo brinquedo
    controlGui = nil
}

-- A interface no nosso HUB
gameTab:AddToggle({ Name = "Ativar Ca√ßa Rei dos C√©us", Default = false, Callback = function(v) kingHunter.ativo = v end })
gameTab:AddSlider({ Name = "Pot√™ncia Horizontal", Min = 50, Max = 500, Default = 150, Callback = function(v) kingHunter.potenciaHorizontal = v end })
gameTab:AddSlider({ Name = "Pot√™ncia Vertical", Min = 50, Max = 300, Default = 100, Callback = function(v) kingHunter.potenciaVertical = v end })
gameTab:AddSlider({ Name = "Velocidade de Curva", Min = 1, Max = 10, Default = 5, Callback = function(v) kingHunter.velocidadeRotacao = v end })

-- O LOOP MESTRE. O √öNICO C√âREBRO. A FONTE DE TODO O PODER.
task.spawn(function()
    while RunService.Heartbeat:Wait() do
        pcall(function()
            local humanoid = LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
            
            if not kingHunter.ativo or not humanoid then
                if kingHunter.controlGui then kingHunter.controlGui:Destroy(); kingHunter.controlGui = nil end
                if kingHunter.estabilizador then kingHunter.estabilizador:Destroy(); kingHunter.estabilizador = nil end
                if kingHunter.motorRotacao then kingHunter.motorRotacao:Destroy(); kingHunter.motorRotacao = nil end
                return
            end

            local currentSeat = humanoid.SeatPart

            if not currentSeat or not currentSeat:IsA("VehicleSeat") then
                if kingHunter.controlGui then kingHunter.controlGui:Destroy(); kingHunter.controlGui = nil end
                if kingHunter.estabilizador then kingHunter.estabilizador:Destroy(); kingHunter.estabilizador = nil end
                if kingHunter.motorRotacao then kingHunter.motorRotacao:Destroy(); kingHunter.motorRotacao = nil end
                return
            end
            
            if not kingHunter.controlGui then
                kingHunter.controlGui = Instance.new("ScreenGui", LocalPlayer.PlayerGui)
                local btnA = Instance.new("TextButton",kingHunter.controlGui); btnA.Size=UDim2.new(0,80,0,80); btnA.Position=UDim2.new(1,-100,1,-190); btnA.BackgroundColor3=Color3.fromRGB(30,200,30); btnA.Text="+"; btnA.TextColor3=Color3.new(1,1,1); btnA.TextSize=60; Instance.new("UICorner",btnA).CornerRadius=UDim.new(1,0)
                local btnF = Instance.new("TextButton",kingHunter.controlGui); btnF.Size=UDim2.new(0,80,0,80); btnF.Position=UDim2.new(1,-100,1,-100); btnF.BackgroundColor3=Color3.fromRGB(200,30,30); btnF.Text="-"; btnF.TextColor3=Color3.new(1,1,1); btnF.TextSize=60; Instance.new("UICorner",btnF).CornerRadius=UDim.new(1,0)
                local btnS = Instance.new("TextButton",kingHunter.controlGui); btnS.Size=UDim2.new(0,80,0,80); btnS.Position=UDim2.new(0,20,1,-190); btnS.BackgroundColor3=Color3.fromRGB(30,100,200); btnS.Text="‚ñ≤"; btnS.TextColor3=Color3.new(1,1,1); btnS.TextSize=60; Instance.new("UICorner",btnS).CornerRadius=UDim.new(1,0)
                local btnD = Instance.new("TextButton",kingHunter.controlGui); btnD.Size=UDim2.new(0,80,0,80); btnD.Position=UDim2.new(0,20,1,-100); btnD.BackgroundColor3=Color3.fromRGB(200,100,30); btnD.Text="‚ñº"; btnD.TextColor3=Color3.new(1,1,1); btnD.TextSize=60; Instance.new("UICorner",btnD).CornerRadius=UDim.new(1,0)
                local btnE = Instance.new("TextButton",kingHunter.controlGui); btnE.Size=UDim2.new(0,80,0,80); btnE.Position=UDim2.new(0,110,1,-100); btnE.BackgroundColor3=Color3.fromRGB(80,80,80); btnE.Text="‚óÑ"; btnE.TextColor3=Color3.new(1,1,1); btnE.TextSize=50; Instance.new("UICorner",btnE).CornerRadius=UDim.new(1,0)
                local btnR = Instance.new("TextButton",kingHunter.controlGui); btnR.Size=UDim2.new(0,80,0,80); btnR.Position=UDim2.new(1,-190,1,-100); btnR.BackgroundColor3=Color3.fromRGB(80,80,80); btnR.Text="‚ñ∫"; btnR.TextColor3=Color3.new(1,1,1); btnR.TextSize=50; Instance.new("UICorner",btnR).CornerRadius=UDim.new(1,0)

                btnA.InputBegan:Connect(function() kingHunter.acelerando = true end); btnA.InputEnded:Connect(function() kingHunter.acelerando = false end)
                btnF.InputBegan:Connect(function() kingHunter.freando = true end); btnF.InputEnded:Connect(function() kingHunter.freando = false end)
                btnS.InputBegan:Connect(function() kingHunter.subindo = true end); btnS.InputEnded:Connect(function() kingHunter.subindo = false end)
                btnD.InputBegan:Connect(function() kingHunter.descendo = true end); btnD.InputEnded:Connect(function() kingHunter.descendo = false end)
                btnE.InputBegan:Connect(function() kingHunter.virandoEsq = true end); btnE.InputEnded:Connect(function() kingHunter.virandoEsq = false end)
                btnR.InputBegan:Connect(function() kingHunter.virandoDir = true end); btnR.InputEnded:Connect(function() kingHunter.virandoDir = false end)
            end           
            local chassi = currentSeat.Parent:FindFirstChild("Body") or currentSeat.Parent:FindFirstChild("Chassis") or currentSeat
            if not chassi then return end
            
            if not kingHunter.estabilizador then
                kingHunter.estabilizador = Instance.new("BodyGyro", chassi); kingHunter.estabilizador.MaxTorque = Vector3.new(9e9, 0, 9e9); kingHunter.estabilizador.P = 50000
            end
            if not kingHunter.motorRotacao then
                kingHunter.motorRotacao = Instance.new("BodyAngularVelocity", chassi); kingHunter.motorRotacao.MaxTorque = Vector3.new(0, 9e9, 0); kingHunter.motorRotacao.P = 10000
            end

            kingHunter.estabilizador.CFrame = CFrame.new(currentSeat.Position, currentSeat.Position + Vector3.new(currentSeat.CFrame.LookVector.X, 0, currentSeat.CFrame.LookVector.Z))
            
            local rotacao = 0
            if kingHunter.virandoEsq then rotacao = kingHunter.velocidadeRotacao elseif kingHunter.virandoDir then rotacao = -kingHunter.velocidadeRotacao end
            kingHunter.motorRotacao.AngularVelocity = Vector3.new(0, rotacao, 0)
            
            if kingHunter.acelerando then kingHunter.velocidadeHorizontal = math.min(kingHunter.potenciaHorizontal, kingHunter.velocidadeHorizontal + (kingHunter.potenciaHorizontal / 45))
            elseif kingHunter.freando then kingHunter.velocidadeHorizontal = math.max(0, kingHunter.velocidadeHorizontal - (kingHunter.potenciaHorizontal / 30))
            else kingHunter.velocidadeHorizontal = kingHunter.velocidadeHorizontal * 0.98 end
            
            if kingHunter.subindo then kingHunter.velocidadeVertical = math.min(kingHunter.potenciaVertical, kingHunter.velocidadeVertical + (kingHunter.potenciaVertical / 45))
            elseif kingHunter.descendo then kingHunter.velocidadeVertical = math.max(-kingHunter.potenciaVertical, kingHunter.velocidadeVertical - (kingHunter.potenciaVertical / 30))
            else kingHunter.velocidadeVertical = kingHunter.velocidadeVertical * 0.95 end

            local direcaoHorizontal = currentSeat.CFrame.LookVector * kingHunter.velocidadeHorizontal
            local direcaoVertical = Vector3.new(0, kingHunter.velocidadeVertical, 0)
            chassi.Velocity = direcaoHorizontal + direcaoVertical
        end)
    end
end)

gameTab:AddSection("[ÊÑõ] - Cabine do Foguete")

--// O c√©rebro do nosso piloto.
local rocketConfig = {
    ativo = false,
    potencia = 200,
    velocidadeAtual = 0,
    acelerando = false,
    freando = false
}

--// A INTERFACE DE CONTROLE NA TELA (A PARTE QUE FALTAVA)
local controlGui = Instance.new("ScreenGui", LocalPlayer.PlayerGui)
controlGui.Name = "DRIP_RocketControls"
controlGui.ResetOnSpawn = false

local btnAcelerar = Instance.new("TextButton", controlGui)
btnAcelerar.Size = UDim2.new(0, 80, 0, 80)
btnAcelerar.Position = UDim2.new(1, -100, 1, -190)
btnAcelerar.BackgroundColor3 = Color3.fromRGB(30, 200, 30); btnAcelerar.Text = "+"; btnAcelerar.TextColor3 = Color3.new(1,1,1); btnAcelerar.TextSize = 60
btnAcelerar.Visible = false
btnAcelerar.AutoButtonColor = false
local cornerA = Instance.new("UICorner", btnAcelerar); cornerA.CornerRadius = UDim.new(1,0)

local btnFrear = Instance.new("TextButton", controlGui)
btnFrear.Size = UDim2.new(0, 80, 0, 80)
btnFrear.Position = UDim2.new(1, -100, 1, -100)
btnFrear.BackgroundColor3 = Color3.fromRGB(200, 30, 30); btnFrear.Text = "-"; btnFrear.TextColor3 = Color3.new(1,1,1); btnFrear.TextSize = 60
btnFrear.Visible = false
btnFrear.AutoButtonColor = false
local cornerB = Instance.new("UICorner", btnFrear); cornerB.CornerRadius = UDim.new(1,0)

-- Eventos de toque/clique para os nossos bot√µes
btnAcelerar.InputBegan:Connect(function(input) if input.UserInputType ~= Enum.UserInputType.MouseMovement then rocketConfig.acelerando = true end end)
btnAcelerar.InputEnded:Connect(function(input) if input.UserInputType ~= Enum.UserInputType.MouseMovement then rocketConfig.acelerando = false end end)
btnFrear.InputBegan:Connect(function(input) if input.UserInputType ~= Enum.UserInputType.MouseMovement then rocketConfig.freando = true end end)
btnFrear.InputEnded:Connect(function(input) if input.UserInputType ~= Enum.UserInputType.MouseMovement then rocketConfig.freando = false end end)


--// A interface no nosso HUB
gameTab:AddToggle({ Name = "Ativar Cabine do Foguete", Default = false, Callback = function(v) rocketConfig.ativo = v end })
gameTab:AddSlider({ Name = "Pot√™ncia M√°xima", Min = 50, Max = 500, Default = 200, Callback = function(v) rocketConfig.potencia = v end })


--// O LOOP MESTRE QUE CONTROLA TUDO
task.spawn(function()
    while true do
        RunService.Heartbeat:Wait() -- Roda a cada frame pra ser responsivo
        if not rocketConfig.ativo then
            btnAcelerar.Visible = false; btnFrear.Visible = false -- Esconde os bot√µes se desligar
            continue
        end

        pcall(function()
            local humanoid = LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
            local seatPart = humanoid and humanoid.SeatPart
            
            -- Se a gente t√° num carro, mostra os bot√µes e liga o motor
            if seatPart and seatPart:IsA("VehicleSeat") then
                btnAcelerar.Visible = true; btnFrear.Visible = true
                
                -- Ajusta a velocidade atual com base nos bot√µes
                if rocketConfig.acelerando then
                    rocketConfig.velocidadeAtual = math.min(rocketConfig.potencia, rocketConfig.velocidadeAtual + (rocketConfig.potencia / 30)) -- Acelera gradualmente
                elseif rocketConfig.freando then
                    rocketConfig.velocidadeAtual = math.max(0, rocketConfig.velocidadeAtual - (rocketConfig.potencia / 20)) -- Freia mais r√°pido
                else
                    rocketConfig.velocidadeAtual = rocketConfig.velocidadeAtual * 0.98 -- Perde velocidade aos poucos
                end

                -- Aplica a for√ßa do foguete no carro
                local direcao = seatPart.CFrame.LookVector
                seatPart.Velocity = direcao * rocketConfig.velocidadeAtual
                
            -- Se n√£o t√° num carro, esconde os bot√µes
            else
                btnAcelerar.Visible = false; btnFrear.Visible = false
                rocketConfig.velocidadeAtual = 0
            end
        end)
    end
end)

gameTab:AddSection("[ÊÑõ] - Oficina de Ve√≠culos")

--// O c√©rebro do nosso mec√¢nico.
local carModConfig = {
    ativo = false,
    velocidade = 200,
    torque = 5000,
    checarCarroAtual = nil
}

--// A interface da nossa oficina.
gameTab:AddToggle({ Name = "Ativar Modificador de Carro", Default = false, Callback = function(v)
    carModConfig.ativo = v
    if not v and carModConfig.checarCarroAtual then
        carModConfig.checarCarroAtual:Disconnect()
        carModConfig.checarCarroAtual = nil
    end
end })
gameTab:AddSlider({ Name = "Velocidade M√°xima", Min = 50, Max = 500, Default = 200, Callback = function(v) carModConfig.velocidade = v end })
gameTab:AddSlider({ Name = "For√ßa do Motor (Torque)", Min = 1000, Max = 20000, Default = 5000, Callback = function(v) carModConfig.torque = v end })

--// O LOOP MESTRE QUE VIRA MEC√ÇNICO
task.spawn(function()
    while true do
        task.wait(0.5)
        if not carModConfig.ativo then continue end

        pcall(function()
            local myChar = LocalPlayer.Character
            if not myChar then return end
            
            local humanoid = myChar:FindFirstChildOfClass("Humanoid")
            if not humanoid then return end
            
            -- Pega o banco do motorista em que a gente t√° sentado
            local vehicleSeat = humanoid.SeatPart
            if not vehicleSeat or not vehicleSeat:IsA("VehicleSeat") then return end
            
            -- A gente achou um carro! Hora de tunar essa lata velha.
            local carro = vehicleSeat:FindFirstAncestorOfClass("Model")
            if not carro then return end

            -- MODIFICA√á√ÉO DE VELOCIDADE
            vehicleSeat.MaxSpeed = carModConfig.velocidade

            -- MODIFICA√á√ÉO DE TORQUE E FOR√áA
            -- A gente ca√ßa todas as pe√ßas que fazem as rodas girarem e bota nitro nelas.
            for _, descendant in ipairs(carro:GetDescendants()) do
                if descendant:IsA("HingeConstraint") and descendant.ActuatorType == Enum.ActuatorType.Motor then
                    descendant.MotorMaxTorque = carModConfig.torque
                    descendant.AngularVelocity = carModConfig.velocidade / 4 -- Uma convers√£o aproximada pra roda acompanhar
                end
                
                -- Alguns jogos usam valores dentro do VehicleSeat
                if descendant:IsA("NumberValue") or descendant:IsA("IntValue") then
                    local name = descendant.Name:lower()
                    if name:find("torque") then
                        descendant.Value = carModConfig.torque
                    elseif name:find("speed") or name:find("velocity") then
                        descendant.Value = carModConfig.velocidade
                    end
                end
            end
        end)
    end
end)

gameTab:AddSection("[ÊÑõ] - Controle do Tempo")
local antiAfkAtivo = false
gameTab:AddToggle({ Name = "Anti-AFK (Nunca ser kickado)", Default = false, Callback = function(state) antiAfkAtivo = state end })
task.spawn(function()
    while task.wait(60) do
        if antiAfkAtivo then
            pcall(function()
                if LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid") then
                    LocalPlayer.Character.Humanoid.Jump = true
                end
            end)
        end
    end
end)
gameTab:AddButton({ Name = "Rejoin (Entrar no servidor de novo)", Callback = function()
    pcall(function() TeleportService:Teleport(game.PlaceId, LocalPlayer) end)
end })

