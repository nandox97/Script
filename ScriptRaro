pcall(function()
    -- Container principal da Ficha, agora mais compacto e elegante
    local FichaRPG = Instance.new("Frame")
    FichaRPG.Size = UDim2.new(1, 0, 0, 220) -- <<< ALTURA REDUZIDA PRA FICAR MAIS CHIQUE
    FichaRPG.BackgroundColor3 = Color3.fromRGB(25, 15, 35)
    FichaRPG.BorderColor3 = Color3.fromRGB(150, 0, 255)
    FichaRPG.BorderSizePixel = 1
    FichaRPG.Parent = inicioTab.Cont
    local cantoFicha = Instance.new("UICorner", FichaRPG)
    cantoFicha.CornerRadius = UDim.new(0, 6)
    
    local paddingFicha = Instance.new("UIPadding", FichaRPG)
    paddingFicha.PaddingLeft, paddingFicha.PaddingRight, paddingFicha.PaddingTop, paddingFicha.PaddingBottom = UDim.new(0, 10), UDim.new(0, 10), UDim.new(0, 10), UDim.new(0, 10)

    -- PAINEL DA ESQUERDA (O MEDALH√ÉO)
    local painelEsquerdo = Instance.new("Frame", FichaRPG)
    painelEsquerdo.Size = UDim2.new(0.4, 0, 1, 0) -- 40% do espa√ßo
    painelEsquerdo.Position = UDim2.new(0, 0, 0, 0)
    painelEsquerdo.BackgroundTransparency = 1
    local layoutEsquerdo = Instance.new("UIListLayout", painelEsquerdo)
    layoutEsquerdo.HorizontalAlignment = Enum.HorizontalAlignment.Center
    layoutEsquerdo.SortOrder = Enum.SortOrder.LayoutOrder
    
    -- PAINEL DA DIREITA (STATUS)
    local painelDireito = Instance.new("Frame", FichaRPG)
    painelDireito.Size = UDim2.new(0.6, -10, 1, 0) -- 60% do espa√ßo
    painelDireito.Position = UDim2.new(0.4, 10, 0, 0)
    painelDireito.BackgroundTransparency = 1
    local layoutDireito = Instance.new("UIListLayout", painelDireito)
    layoutDireito.SortOrder = Enum.SortOrder.LayoutOrder
    layoutDireito.Padding = UDim.new(0, 8)

    -- O CORA√á√ÉO DA M√ÅGICA: O MEDALH√ÉO
    local containerMedalhao = Instance.new("Frame", painelEsquerdo)
    containerMedalhao.LayoutOrder = 1
    containerMedalhao.Size = UDim2.new(1, 0, 0.7, 0) -- Ocupa 70% da altura do painel esquerdo
    containerMedalhao.BackgroundTransparency = 1

    local estandarteImg = Instance.new("ImageLabel", containerMedalhao)
    estandarteImg.Size = UDim2.new(1, 0, 1, 0)
    estandarteImg.Position = UDim2.new(0.5, 0, 0.5, 0)
    estandarteImg.AnchorPoint = Vector2.new(0.5, 0.5)
    estandarteImg.BackgroundColor3 = Color3.fromRGB(15, 5, 25)
    estandarteImg.ScaleType = Enum.ScaleType.Crop
    estandarteImg.Image = "rbxassetid://110099896917917"
    
    -- AQUI A GENTE FOR√áA O MEDALH√ÉO A SER UM C√çRCULO PERFEITO
    local aspectRatio = Instance.new("UIAspectRatioConstraint", estandarteImg)
    aspectRatio.AspectRatio = 1 -- Propor√ß√£o 1:1 (quadrado)
    local cantoCircular = Instance.new("UICorner", estandarteImg)
    cantoCircular.CornerRadius = UDim.new(1, 0) -- Raio 100% (c√≠rculo)

    -- NOME DO CHEFE ABAIXO DO MEDALH√ÉO
    local nomePrincipalLabel = Instance.new("TextLabel", painelEsquerdo)
    nomePrincipalLabel.LayoutOrder = 2
    nomePrincipalLabel.Size = UDim2.new(1, 0, 0, 25)
    nomePrincipalLabel.Font = Enum.Font.GothamBold
    nomePrincipalLabel.Text = LocalPlayer.Name
    nomePrincipalLabel.TextColor3 = Color3.fromRGB(240, 240, 240)
    nomePrincipalLabel.TextSize = 18
    nomePrincipalLabel.BackgroundTransparency = 1

    local subtituloNomeLabel = Instance.new("TextLabel", painelEsquerdo)
    subtituloNomeLabel.LayoutOrder = 3
    subtituloNomeLabel.Size = UDim2.new(1, 0, 0, 15)
    subtituloNomeLabel.Font = Enum.Font.Gotham
    subtituloNomeLabel.Text = "Chefe no Comando üêâ"
    subtituloNomeLabel.TextColor3 = Color3.fromRGB(180, 150, 255)
    subtituloNomeLabel.TextSize = 12
    subtituloNomeLabel.BackgroundTransparency = 1
    
    -- FUN√á√ÉO PARA CRIAR OS R√ìTULOS DE STATUS
    local function criarRotuloStatus(nome, ordem)
        local frame = Instance.new("Frame", painelDireito); frame.LayoutOrder = ordem; frame.Size = UDim2.new(1, 0, 0, 25); frame.BackgroundTransparency = 1
        local nomeLabel = Instance.new("TextLabel", frame); nomeLabel.Size = UDim2.new(0.5, 0, 1, 0); nomeLabel.Font = Enum.Font.GothamBold; nomeLabel.TextColor3 = Color3.fromRGB(180, 150, 255); nomeLabel.Text = nome .. ":"; nomeLabel.TextXAlignment = Enum.TextXAlignment.Left; nomeLabel.TextSize = 16; nomeLabel.BackgroundTransparency = 1
        local valorLabel = Instance.new("TextLabel", frame); valorLabel.Size = UDim2.new(0.5, 0, 1, 0); valorLabel.Position = UDim2.new(0.5, 0, 0, 0); valorLabel.Font = Enum.Font.Gotham; valorLabel.TextColor3 = Color3.fromRGB(240, 240, 240); valorLabel.Text = "..."; valorLabel.TextXAlignment = Enum.TextXAlignment.Right; valorLabel.TextSize = 16; valorLabel.BackgroundTransparency = 1
        return valorLabel
    end

    -- Cria os r√≥tulos (sem o nome, que agora t√° na esquerda)
    local vidaValor = criarRotuloStatus("VIDA", 1)
    local pingValor = criarRotuloStatus("PING", 2)
    local fpsValor = criarRotuloStatus("FPS", 3)
    local playersValor = criarRotuloStatus("JOGADORES", 4)

    -- LOOP DE ATUALIZA√á√ÉO DOS STATUS (INTOCADO)
    task.spawn(function()
        while task.wait(1) do
            if FichaRPG and FichaRPG.Parent and FichaRPG.Visible then
                pcall(function() local hum = LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid"); if hum then vidaValor.Text = string.format("%d / %d", math.floor(hum.Health), math.floor(hum.MaxHealth)) end end)
                pcall(function() pingValor.Text = game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString() end)
                pcall(function() fpsValor.Text = tostring(math.floor(workspace:GetRealPhysicsFPS())) end)
                pcall(function() playersValor.Text = string.format("%d / %d", #Players:GetPlayers(), Players.MaxPlayers) end)
            end
        end
    end)
    
    -- PAINEL DE CUSTOMIZA√á√ÉO (INTOCADO)
    inicioTab:AddSection("[ÊÑõ] - Fotinha bb")
    local imagemId = ""
    inicioTab:AddTextBox({Name = "ID da Imagem (rbxassetid)", PlaceholderText = "Cole o ID da imagem aqui...", Callback = function(v) imagemId = v end})
    inicioTab:AddButton({Name = "ATUALIZAR IMAGEM", Callback = function() if imagemId and tonumber(imagemId) and estandarteImg then estandarteImg.Image = "rbxassetid://" .. imagemId end end})
end)
