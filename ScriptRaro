--[=[
    DRIP - EXPANS√ÉO DA F√ÅBRICA üè≠
    PARTE 9: Novos Bot√µes.
    Adicionando os novos interruptores na nossa central de comando.
]=]

--// ADICIONANDO OS NOVOS BOT√ïES NA INTERFACE, L√Å NA SE√á√ÉO DE "COMPONENTES VISUAIS"
espTab:AddToggle({ Name = "Head ESP (C√≠rculo)", Default = false, Callback = function(v) espConfig.Head = v end })
espTab:AddToggle({ Name = "Dist√¢ncia", Default = false, Callback = function(v) espConfig.Distancia = v end })
espTab:AddToggle({ Name = "Item Segurado", Default = false, Callback = function(v) espConfig.Item = v end })
espTab:AddToggle({ Name = "Dire√ß√£o da Mira (Aim)", Default = false, Callback = function(v) espConfig.Aim = v end })


--// E A GENTE ADICIONA AS NOVAS OP√á√ïES NO NOSSO C√âREBRO
espConfig.Head = false
espConfig.Distancia = false
espConfig.Item = false
espConfig.Aim = false

--[=[
    DRIP - CLONAGEM PROFISSIONAL üß¨
    PARTE 2: A F√°brica de Desenhos (Box e Esqueleto).
    Clonando as primeiras m√°quinas da linha de produ√ß√£o.
]=]

--// FUN√á√ïES DE CRIA√á√ÉO UNIFICADAS (CLONADAS 100% DA TUA BASE)
local function criarDesenhosBox(player)
    if not espDrawings[player] then espDrawings[player] = {} end
    if espDrawings[player].Box then return end
    espDrawings[player].Box = Drawing.new("Square")
    local box = espDrawings[player].Box
    box.Thickness = 1; box.Transparency = 1; box.Filled = false; box.Visible = false
end

local bodyConnections = {
    R15 = { {"Head","UpperTorso"}, {"UpperTorso","LowerTorso"}, {"LowerTorso","LeftUpperLeg"},
           {"LowerTorso","RightUpperLeg"}, {"LeftUpperLeg","LeftLowerLeg"}, {"LeftLowerLeg","LeftFoot"},
           {"RightUpperLeg","RightLowerLeg"}, {"RightUpperLeg","RightFoot"}, {"UpperTorso","LeftUpperArm"},
           {"UpperTorso","RightUpperArm"}, {"LeftUpperArm","LeftLowerArm"}, {"LeftLowerArm","LeftHand"},
           {"RightUpperArm","RightLowerArm"}, {"RightLowerArm","RightHand"} },
    R6  = { {"Head","Torso"}, {"Torso","Left Arm"}, {"Torso","Right Arm"}, {"Torso","Left Leg"}, {"Torso","Right Leg"} }
}

local function criarDesenhosEsqueleto(player)
    if not espDrawings[player] then espDrawings[player] = {} end
    if espDrawings[player].Esqueleto then return end
    local rigType = player.Character and player.Character:FindFirstChild("Humanoid") and player.Character.Humanoid.RigType.Name
    local connections = rigType and bodyConnections[rigType]
    if connections then
        local lines = {}
        for _ in ipairs(connections) do
            local line = Drawing.new("Line")
            line.Thickness = 1; line.Visible = false
            table.insert(lines, line)
        end
        espDrawings[player].Esqueleto = lines
    end
end
--[=[
    DRIP - CLONAGEM PROFISSIONAL üß¨
    PARTE 3: A F√°brica de Desenhos (Tracer e Info).
    Finalizando a nossa linha de produ√ß√£o de armas visuais.
]=]

local function criarDesenhosTracer(player)
    if not espDrawings[player] then espDrawings[player] = {} end
    if espDrawings[player].Tracer then return end
    espDrawings[player].Tracer = Drawing.new("Line")
end

local function criarDesenhosInfo(player)
    if not espDrawings[player] then espDrawings[player] = {} end
    if espDrawings[player].Username then return end
    espDrawings[player].Username = Drawing.new("Text")
    espDrawings[player].DisplayName = Drawing.new("Text")
    espDrawings[player].VidaBarraFundo = Drawing.new("Square")
    espDrawings[player].VidaBarraFrente = Drawing.new("Square")
end
--[=[
    DRIP - CLONAGEM PROFISSIONAL üß¨
    PARTE 4: O Loop do Box ESP.
    A primeira esteira de produ√ß√£o t√° ligada.
]=]

--// Loop principal de ESP geral (boxes) - (L√ìGICA CLONADA)
RunService.RenderStepped:Connect(function()
    if not espConfig.Ativo then
        for _, drawings in pairs(espDrawings) do
            if drawings.Box and drawings.Box.Visible then
                drawings.Box.Visible = false
            end
        end
        return
    end

    for player, drawings in pairs(espDrawings) do
        if not player.Parent or not player.Character or (player.Character:FindFirstChild("Humanoid") and player.Character.Humanoid.Health <= 0) then
            for _, obj in pairs(drawings) do
                if type(obj) == "table" then
                    for _, sub in ipairs(obj) do if sub and sub.Remove then sub:Remove() end end
                elseif obj and obj.Remove then
                    obj:Remove()
                end
            end
            espDrawings[player] = nil
        end
    end

    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer then
            local isEnemy = not espConfig.ApenasInimigos or (player.Team ~= LocalPlayer.Team)
            if isEnemy and player.Character and player.Character:FindFirstChild("HumanoidRootPart") and player.Character.Humanoid.Health > 0 then
                local hrp = player.Character.HumanoidRootPart
                local distancia = (camera.CFrame.Position - hrp.Position).Magnitude
                if distancia <= espConfig.DistanciaMaxima then
                    if espConfig.Box then
                        criarDesenhosBox(player)
                        if espDrawings[player] and espDrawings[player].Box then
                            local box = espDrawings[player].Box
                            local cor = espConfig.UsarCoresDoTime and (player.Team and player.Team.TeamColor.Color or Color3.new(1,1,1)) or Color3.fromRGB(170, 0, 255)
                            local rootPos, naTela = camera:WorldToViewportPoint(hrp.Position)
                            if naTela then
                                local boxSize = Vector2.new(2000 / distancia, 3000 / distancia)
                                box.Size = boxSize
                                box.Position = Vector2.new(rootPos.X - boxSize.X/2, rootPos.Y - boxSize.Y/2)
                                box.Color = cor
                                box.Visible = true
                            else
                                box.Visible = false
                            end
                        end
                    elseif espDrawings[player] and espDrawings[player].Box and espDrawings[player].Box.Visible then
                        espDrawings[player].Box.Visible = false
                    end
                else
                    if espDrawings[player] and espDrawings[player].Box and espDrawings[player].Box.Visible then
                        espDrawings[player].Box.Visible = false
                    end
                end
            end
        end
    end
end)
--[=[
    DRIP - CLONAGEM PROFISSIONAL üß¨
    PARTE 5: O Loop do Esqueleto ESP.
    A segunda esteira de produ√ß√£o t√° ligada.
]=]

--// Loop de ESQUELETO - (L√ìGICA CLONADA)
RunService.RenderStepped:Connect(function()
    if not espConfig.Ativo or not espConfig.Esqueleto then
        for _, drawings in pairs(espDrawings) do
            if drawings.Esqueleto then
                for _, line in ipairs(drawings.Esqueleto) do
                    line.Visible = false
                end
            end
        end
        return
    end

    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer then
            local isEnemy = not espConfig.ApenasInimigos or (player.Team ~= LocalPlayer.Team)
            local hrp = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
            local humanoid = player.Character and player.Character:FindFirstChild("Humanoid")
            
            if isEnemy and hrp and humanoid and humanoid.Health > 0 then
                local distancia = (camera.CFrame.Position - hrp.Position).Magnitude
                if distancia <= espConfig.DistanciaMaxima then
                    criarDesenhosEsqueleto(player)
                    local drawings = espDrawings[player]
                    local cor = espConfig.UsarCoresDoTime and (player.Team and player.Team.TeamColor.Color or Color3.new(1,1,1)) or Color3.fromRGB(170, 0, 255)
                    local rigType = humanoid.RigType.Name
                    local connections = bodyConnections[rigType]

                    if drawings and drawings.Esqueleto and connections then
                        for i, conn in ipairs(connections) do
                            local partA = player.Character:FindFirstChild(conn[1])
                            local partB = player.Character:FindFirstChild(conn[2])
                            local line = drawings.Esqueleto[i]
                            if partA and partB and line then
                                local posA, onScreenA = camera:WorldToViewportPoint(partA.Position)
                                local posB, onScreenB = camera:WorldToViewportPoint(partB.Position)
                                if onScreenA and onScreenB then
                                    line.From = Vector2.new(posA.X, posA.Y)
                                    line.To = Vector2.new(posB.X, posB.Y)
                                    line.Color = cor
                                    line.Visible = true
                                else
                                    line.Visible = false
                                end
                            end
                        end
                    end
                else
                    if espDrawings[player] and espDrawings[player].Esqueleto then
                        for _, line in ipairs(espDrawings[player].Esqueleto) do line.Visible = false end
                    end
                end
            else
                if espDrawings[player] and espDrawings[player].Esqueleto then
                    for _, line in ipairs(espDrawings[player].Esqueleto) do line.Visible = false end
                end
            end
        end
    end
end)
--[=[
    DRIP - CLONAGEM PROFISSIONAL üß¨
    PARTE 6: O Loop dos Tracers.
    A terceira esteira de produ√ß√£o t√° ligada. O GPS da Morte t√° online.
]=]

--// Loop de TRACERS - (L√ìGICA CLONADA)
RunService.RenderStepped:Connect(function()
    if not espConfig.Ativo or not espConfig.Tracers then
        for _, drawings in pairs(espDrawings) do
            if drawings.Tracer then
                drawings.Tracer.Visible = false
            end
        end
        return
    end

    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer then
            local isEnemy = not espConfig.ApenasInimigos or (player.Team ~= LocalPlayer.Team)
            local hrp = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
            local humanoid = player.Character and player.Character:FindFirstChild("Humanoid")

            if isEnemy and hrp and humanoid and humanoid.Health > 0 then
                local distancia = (camera.CFrame.Position - hrp.Position).Magnitude
                if distancia <= espConfig.DistanciaMaxima then
                    criarDesenhosTracer(player)
                    local drawings = espDrawings[player]
                    if drawings and drawings.Tracer then
                        local cor = espConfig.UsarCoresDoTime and (player.Team and player.Team.TeamColor.Color or Color3.new(1,1,1)) or Color3.fromRGB(170, 0, 255)
                        local rootPos, naTela = camera:WorldToViewportPoint(hrp.Position)
                        if naTela then
                            local tracer = drawings.Tracer
                            tracer.From = Vector2.new(camera.ViewportSize.X / 2, camera.ViewportSize.Y)
                            tracer.To = Vector2.new(rootPos.X, rootPos.Y)
                            tracer.Color = cor
                            tracer.Thickness = 1
                            tracer.Visible = true
                        else
                            drawings.Tracer.Visible = false
                        end
                    end
                else
                    if espDrawings[player] and espDrawings[player].Tracer then
                        espDrawings[player].Tracer.Visible = false
                    end
                end
            else
                if espDrawings[player] and espDrawings[player].Tracer then
                    espDrawings[player].Tracer.Visible = false
                end
            end
        end
    end
end)
--[=[
    DRIP - CLONAGEM PROFISSIONAL üß¨
    PARTE 7: O Loop das Informa√ß√µes.
    A √∫ltima e mais importante esteira de produ√ß√£o. O Dedo-Duro t√° online.
]=]
--// Loop de USERNAME/DISPLAY/Vida - (L√ìGICA CLONADA)
RunService.RenderStepped:Connect(function()
    local algumaInfoAtiva = espConfig.Username or espConfig.DisplayName or espConfig.Vida
    if not espConfig.Ativo or not algumaInfoAtiva then
        for _, drawings in pairs(espDrawings) do
            if drawings.Username then drawings.Username.Visible = false end
            if drawings.DisplayName then drawings.DisplayName.Visible = false end
            if drawings.VidaBarraFundo then drawings.VidaBarraFundo.Visible = false; drawings.VidaBarraFrente.Visible = false end
        end
        return
    end

    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer then
            local isEnemy = not espConfig.ApenasInimigos or (player.Team ~= LocalPlayer.Team)
            local hrp = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
            local humanoid = player.Character and player.Character:FindFirstChild("Humanoid")
            if isEnemy and hrp and humanoid and humanoid.Health > 0 then
                local distancia = (camera.CFrame.Position - hrp.Position).Magnitude
                if distancia <= espConfig.DistanciaMaxima then
                    criarDesenhosInfo(player)
                    local drawings = espDrawings[player]
                    if drawings then
                        local rootPos, naTela = camera:WorldToViewportPoint(hrp.Position)
                        if naTela then
                            local yOffset = -30
                            
                            if espConfig.Username then
                                drawings.Username.Visible = true; drawings.Username.Text = player.Name; drawings.Username.Color = Color3.new(1,1,1); drawings.Username.Size = 14; drawings.Username.Center = true; drawings.Username.Outline = true; drawings.Username.Position = Vector2.new(rootPos.X, rootPos.Y + yOffset)
                                yOffset = yOffset - 15
                            elseif drawings.Username then drawings.Username.Visible = false end
                            
                            if espConfig.DisplayName then
                                drawings.DisplayName.Visible = true; drawings.DisplayName.Text = player.DisplayName; drawings.DisplayName.Color = Color3.new(0.8,0.8,0.8); drawings.DisplayName.Size = 14; drawings.DisplayName.Center = true; drawings.DisplayName.Outline = true; drawings.DisplayName.Position = Vector2.new(rootPos.X, rootPos.Y + yOffset)
                                yOffset = yOffset - 15
                            elseif drawings.DisplayName then drawings.DisplayName.Visible = false end
                            
                            if espConfig.Vida then
                                local vidaPercent = humanoid.Health / humanoid.MaxHealth
                                local barraWidth, barraHeight = 50, 5
                                drawings.VidaBarraFundo.Visible = true; drawings.VidaBarraFundo.Size = Vector2.new(barraWidth+2, barraHeight+2); drawings.VidaBarraFundo.Position = Vector2.new(rootPos.X-(barraWidth/2)-1, rootPos.Y+yOffset-1); drawings.VidaBarraFundo.Color = Color3.new(0,0,0)
                                drawings.VidaBarraFrente.Visible = true; drawings.VidaBarraFrente.Size = Vector2.new(barraWidth*vidaPercent, barraHeight); drawings.VidaBarraFrente.Position = Vector2.new(rootPos.X-(barraWidth/2), rootPos.Y+yOffset); drawings.VidaBarraFrente.Color = Color3.fromHSV(vidaPercent*0.33,1,1)
                            elseif drawings.VidaBarraFundo then drawings.VidaBarraFundo.Visible = false; drawings.VidaBarraFrente.Visible = false end
                        else
                            if drawings.Username then drawings.Username.Visible = false end
                            if drawings.DisplayName then drawings.DisplayName.Visible = false end
                            if drawings.VidaBarraFundo then drawings.VidaBarraFundo.Visible = false; drawings.VidaBarraFrente.Visible = false end
                        end
                    end
                end
            else
                if espDrawings[player] then
                    if espDrawings[player].Username then espDrawings[player].Username.Visible = false end
                    if espDrawings[player].DisplayName then espDrawings[player].DisplayName.Visible = false end
                    if espDrawings[player].VidaBarraFundo then espDrawings[player].VidaBarraFundo.Visible = false; espDrawings[player].VidaBarraFrente.Visible = false end
                end
            end
        end
    end
end)
--[=[
    DRIP - EXPANS√ÉO DA F√ÅBRICA üè≠
    PARTE 10: O Scanner T√°tico.
    A l√≥gica final. A gente agora v√™ tudo.
]=]

--// ATUALIZANDO A F√ÅBRICA DE DESENHOS UMA √öLTIMA VEZ
local function criarDesenhosParaPlayer(player)
    if not espDrawings[player] then espDrawings[player] = {} end
    if espDrawings[player].Head then return end -- Se j√° tem um, j√° tem todos os novos
    
    espDrawings[player].Head = Drawing.new("Circle")
    espDrawings[player].Distancia = Drawing.new("Text")
    espDrawings[player].Item = Drawing.new("Text")
    espDrawings[player].Aim = Drawing.new("Line")
end

--// O CORA√á√ÉO DO SCANNER T√ÅTICO (SEU PR√ìPRIO LOOP DEDICADO)
RunService.RenderStepped:Connect(function()
    local algumaAtiva = espConfig.Head or espConfig.Distancia or espConfig.Item or espConfig.Aim
    if not espConfig.Ativo or not algumaAtiva then
        for _, drawings in pairs(espDrawings) do
            if drawings.Head and drawings.Head.Visible then drawings.Head.Visible = false end
            if drawings.Distancia and drawings.Distancia.Visible then drawings.Distancia.Visible = false end
            if drawings.Item and drawings.Item.Visible then drawings.Item.Visible = false end
            if drawings.Aim and drawings.Aim.Visible then drawings.Aim.Visible = false end
        end
        return
    end

    for _, player in ipairs(Players:GetPlayers()) do
        local alvoValido = (not espConfig.ApenasInimigos or (player.Team ~= LocalPlayer.Team))
        local char = player.Character
        local head = char and char:FindFirstChild("Head")
        
        if alvoValido and head and char.Humanoid.Health > 0 then
            local distancia = (camera.CFrame.Position - head.Position).Magnitude
            if distancia <= espConfig.DistanciaMaxima then
                criarDesenhosParaPlayer(player)
                local drawings = espDrawings[player]
                local cor = espConfig.UsarCoresDoTime and (player.Team and player.Team.TeamColor.Color or Color3.new(1,1,1)) or Color3.fromRGB(170, 0, 255)
                local headPos, naTela = camera:WorldToViewportPoint(head.Position)
                
                if naTela then
                    local yOffset = -50 -- A gente bota as infos novas um pouco mais pra baixo
                    
                    -- DESENHA O C√çRCULO NA CABE√áA
                    if espConfig.Head and drawings.Head then
                        drawings.Head.Visible = true; drawings.Head.Radius = 300 / distancia; drawings.Head.Color = cor; drawings.Head.Thickness = 1; drawings.Head.Filled = false
                        drawings.Head.Position = Vector2.new(headPos.X, headPos.Y)
                    elseif drawings.Head then drawings.Head.Visible = false end
                    
                    -- DESENHA A DIST√ÇNCIA
                    if espConfig.Distancia and drawings.Distancia then
                        drawings.Distancia.Visible = true; drawings.Distancia.Text = "[" .. math.floor(distancia) .. "m]"; drawings.Distancia.Color = Color3.new(1,1,1); drawings.Distancia.Size = 14; drawings.Distancia.Center = true; drawings.Distancia.Outline = true
                        drawings.Distancia.Position = Vector2.new(headPos.X, headPos.Y + yOffset); yOffset = yOffset - 15
                    elseif drawings.Distancia then drawings.Distancia.Visible = false end
                    
                    -- DESENHA O ITEM SEGURADO
                    if espConfig.Item and drawings.Item then
                        local item = char:FindFirstChildOfClass("Tool")
                        if item then
                            drawings.Item.Visible = true; drawings.Item.Text = item.Name; drawings.Item.Color = Color3.fromRGB(0, 255, 255); drawings.Item.Size = 14; drawings.Item.Center = true; drawings.Item.Outline = true
                            drawings.Item.Position = Vector2.new(headPos.X, headPos.Y + yOffset); yOffset = yOffset - 15
                        elseif drawings.Item.Visible then drawings.Item.Visible = false end
                    elseif drawings.Item then drawings.Item.Visible = false end
                    
                    -- DESENHA A DIRE√á√ÉO DA MIRA
                    if espConfig.Aim and drawings.Aim then
                        local lookVector = head.CFrame.LookVector
                        local aimEndPos = head.Position + (lookVector * 15) -- O tamanho da linha da mira
                        local aimScreenPos, aimOnScreen = camera:WorldToViewportPoint(aimEndPos)
                        
                        if aimOnScreen then
                            drawings.Aim.Visible = true; drawings.Aim.From = Vector2.new(headPos.X, headPos.Y); drawings.Aim.To = Vector2.new(aimScreenPos.X, aimScreenPos.Y)
                            drawings.Aim.Color = Color3.fromRGB(255,0,0); drawings.Aim.Thickness = 2
                        else drawings.Aim.Visible = false end
                    elseif drawings.Aim then drawings.Aim.Visible = false end
                else
                    if drawings.Head then drawings.Head.Visible = false end; if drawings.Distancia then drawings.Distancia.Visible = false end; if drawings.Item then drawings.Item.Visible = false end; if drawings.Aim then drawings.Aim.Visible = false end
                end
            end
        end
    end
end)        
--[=[
    DRIP - CLONAGEM PROFISSIONAL üß¨
    PARTE 8: A Limpeza Final.
    A equipe de limpeza que garante que a gente n√£o deixe rastros.
]=]

--// EVENTO: jogador removendo - limpa todos os desenhos restantes (L√ìGICA CLONADA)
Players.PlayerRemoving:Connect(function(player)
    local drawings = espDrawings[player]
    if drawings then
        for _, obj in pairs(drawings) do
            if type(obj) == "table" then
                for _, sub_obj in ipairs(obj) do
                    if sub_obj and sub_obj.Remove then sub_obj:Remove() end
                end
            elseif obj and obj.Remove then
                obj:Remove()
            end
        end
        espDrawings[player] = nil
    end
end)




--[=[
    DRIP - A SIMPLICIDADE DO PODER üëë
    Um bot√£o. Um clique. A m√°gica acontece.
    A pedido do Chefe, a vers√£o mais limpa e direta.
]=]

