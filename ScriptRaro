-- AQUI A GENTE SOLTA O NOSSO VIGIA.
-- Ele vai ca√ßar o painel do RedzLib e apagar a luz dele na hora certa.
task.spawn(function()
    local redzScreenGui = nil
    -- O nosso vigia vai fi at√© o alvo aparecer.
    repeat
        redzScreenGui = game:GetService("CoreGui"):FindFirstChild("redz Library V5")
        if not redzScreenGui then
            game:GetService("RunService").Heartbeat:Wait() -- Espera um piscar de olhos e olha de novo.
        end
    until redzScreenGui -- Achou!

    -- ALVO ENCONTRADO. LUZES APAGADAS.
    redzScreenGui.Enabled = false
end)


-- O resto do teu c√≥digo, come√ßando com a linha "local redzlib = loadstring(...)", vem depois.

local redzlib = loadstring(game:HttpGet("https://raw.githubusercontent.com/SrDark222/SH_0001/refs/heads/main/inject%20R.Hub.lua"))()
pcall(function()
    -- Captura o tema Darker atual
    local temaSequestrado = redzlib.Themes.Darker

    -- Azul escuro poderoso como cor principal
    temaSequestrado["Color Theme"] = Color3.fromRGB(0, 85, 170)  

    -- Fundo com degrad√™ azul profundo ‚Üí preto
    temaSequestrado["Color Hub 1"] = ColorSequence.new({
        ColorSequenceKeypoint.new(0.00, Color3.fromRGB(10, 10, 20)),   -- Preto quase absoluto nas bordas
        ColorSequenceKeypoint.new(0.50, Color3.fromRGB(0, 40, 100)),  -- Azul profundo no meio
        ColorSequenceKeypoint.new(1.00, Color3.fromRGB(10, 10, 20))   -- Fecha no preto forte
    })

    -- Fundo s√≥lido secund√°rio (preto forte com toque de azul)
    temaSequestrado["Color Hub 2"] = Color3.fromRGB(15, 15, 25)

    -- Ajuste extra: textos brancos pra contraste
    temaSequestrado["Text Color"] = Color3.fromRGB(240, 240, 240)

    -- Ajuste extra: bordas em azul mais vivo pra destacar
    temaSequestrado["Outline Color"] = Color3.fromRGB(0, 120, 255)
end)
