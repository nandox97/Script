animationTab:AddSection("[ÊÑõ] - Ativador de Anima√ß√£o Customizada")

local motionaLoaded = false

animationTab:AddButton({
    Name = "ATIVAR MOTOR DE ANIMA√á√ÉO (MOTIONA)",
    Callback = function()
        if motionaLoaded then
            warn("DRIP: O motor Motiona j√° foi carregado.")
            return
        end
        
        local success, err = pcall(function()
            -- A gente carrega e executa o script que VOC√ä achou.
            loadstring(game:HttpGet("https://raw.githubusercontent.com/BeemTZy/Motiona/refs/heads/main/source.lua"))()
        end)
        
        if success then
            print("DRIP: Motor Motiona carregado com sucesso! Use o painel deles para trocar as anima√ß√µes.")
            motionaLoaded = true
        else
            warn("DRIP: Falha cr√≠tica ao carregar o motor Motiona. O script pode estar offline ou quebrado.")
            warn("Erro: " .. tostring(err))
        end
    end
})

animationTab:AddParagraph({
    "Instru√ß√µes:",
    "1. Clique no bot√£o acima UMA VEZ.",
    "2. O script do Motiona vai criar seu pr√≥prio painel na tela.",
    "3. Use o painel DELES para escolher e customizar as anima√ß√µes.",
    "Nosso Hub apenas ativa o poder deles para voc√™."
})

