playerTab:AddSection("üëª Proje√ß√£o Astral (Vers√£o Final)")

--// O c√©rebro do nosso fantasma.
local fantasmaConfig = {
    ativo = false,
    corpoFalso = nil,
    conexaoRender = nil,
    posicaoOriginal = nil,
    bunker = nil,
    controlGui = nil -- A gente vai guardar a refer√™ncia aqui
}

--// A GENTE CRIA A PORRA DA UI UMA VEZ E ESCONDE ELA.
local controlGui = Instance.new("ScreenGui", CoreGui)
controlGui.Name = "DRIP_FANTASMA_UI_FINAL"
controlGui.ResetOnSpawn = false
controlGui.Enabled = false -- Come√ßa desligada e invis√≠vel.
fantasmaConfig.controlGui = controlGui

local toggleButton = Instance.new("TextButton", controlGui)
toggleButton.Size = UDim2.fromOffset(180, 45)
toggleButton.Position = UDim2.new(0.5, -90, 0, 10)
toggleButton.BackgroundColor3 = Color3.fromRGB(50, 200, 150)
toggleButton.Text = "PROJE√á√ÉO ATIVA"
toggleButton.Font = Enum.Font.GothamBold
toggleButton.TextColor3 = Color3.new(1, 1, 1)
toggleButton.TextSize = 16
Instance.new("UICorner", toggleButton).CornerRadius = UDim.new(0, 8)

--// O clique no bot√£o da tela desliga a fun√ß√£o principal no Hub
toggleButton.MouseButton1Click:Connect(function()
    pcall(function()
        Window.Tabs.Player.Container.Children["Ativar Proje√ß√£o Astral"].State = false
    end)
end)


--// FUN√á√ÉO PRA ATIVAR (A l√≥gica que j√° tava boa)
local function ativarProjecao()
    local char = LocalPlayer.Character
    local hrp = char and char:FindFirstChild("HumanoidRootPart")
    local humanoid = char and char:FindFirstChildOfClass("Humanoid")
    if not (hrp and humanoid) then return end

    if not fantasmaConfig.bunker then
        fantasmaConfig.bunker = Instance.new("Part", workspace)
        fantasmaConfig.bunker.Name = "DRIP_BunkerDoChefe_FINAL"; fantasmaConfig.bunker.Size = Vector3.new(10,10,10)
        fantasmaConfig.bunker.Anchored = true; fantasmaConfig.bunker.CanCollide = true; fantasmaConfig.bunker.Transparency = 1
        fantasmaConfig.bunker.Position = Vector3.new(0, 15000, 0) -- Mais alto ainda, foda-se
    end
    
    fantasmaConfig.posicaoOriginal = hrp.CFrame
    hrp.CFrame = fantasmaConfig.bunker.CFrame
    humanoid.PlatformStand = true

    fantasmaConfig.corpoFalso = Instance.new("Part", workspace)
    fantasmaConfig.corpoFalso.Name = "DRIP_Alma_FINAL"; fantasmaConfig.corpoFalso.Size = Vector3.new(1,1,1)
    fantasmaConfig.corpoFalso.Anchored = true; fantasmaConfig.corpoFalso.CanCollide = false; fantasmaConfig.corpoFalso.Transparency = 1
    fantasmaConfig.corpoFalso.CFrame = fantasmaConfig.posicaoOriginal

    camera.CameraType = Enum.CameraType.Scriptable

    fantasmaConfig.conexaoRender = RunService.RenderStepped:Connect(function()
        if not fantasmaConfig.ativo or not fantasmaConfig.corpoFalso or not humanoid then return end
        fantasmaConfig.corpoFalso.CFrame = fantasmaConfig.corpoFalso.CFrame * CFrame.new(humanoid.MoveDirection * humanoid.WalkSpeed * 0.02)
        local posCamera = fantasmaConfig.corpoFalso.Position + Vector3.new(0, 5, 10)
        camera.CFrame = CFrame.new(posCamera, fantasmaConfig.corpoFalso.Position)
    end)
end

--// FUN√á√ÉO PRA DESATIVAR (A l√≥gica que j√° tava boa)
local function desativarProjecao()
    if fantasmaConfig.conexaoRender then fantasmaConfig.conexaoRender:Disconnect(); fantasmaConfig.conexaoRender = nil end
    local char = LocalPlayer.Character
    local hrp = char and char:FindFirstChild("HumanoidRootPart")
    local humanoid = char and char:FindFirstChildOfClass("Humanoid")
    if hrp and fantasmaConfig.corpoFalso then hrp.CFrame = fantasmaConfig.corpoFalso.CFrame end
    if humanoid then humanoid.PlatformStand = false end
    if fantasmaConfig.corpoFalso then fantasmaConfig.corpoFalso:Destroy(); fantasmaConfig.corpoFalso = nil end
    camera.CameraType = Enum.CameraType.Custom
    camera.CameraSubject = humanoid
end

--// O TOGGLE NO HUB QUE √â S√ì UM INTERRUPTOR DE LUZ, PORRA!
playerTab:AddToggle({
    Name = "Ativar Proje√ß√£o Astral",
    Default = false,
    Callback = function(state)
        fantasmaConfig.ativo = state
        
        -- A L√ìGICA MAIS SIMPLES DO MUNDO
        fantasmaConfig.controlGui.Enabled = state -- LIGA OU DESLIGA A LUZ
        
        if state then
            ativarProjecao()
        else
            desativarProjecao()
        end
    end
})

