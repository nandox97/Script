gameTab:AddButton({ Name = "TENTAR RESGATAR TODOS OS C√ìDIGOS", Callback = function()
    task.spawn(function()
        for _, codigo in ipairs(codigosParaResgatar) do
            pcall(function()
                local remote = ReplicatedStorage:FindFirstChild("Default", true) or ReplicatedStorage:FindFirstChild("Remote", true)
                if remote and remote:FindFirstChild("RedeemCode", true) then
                    remote.RedeemCode:FireServer(codigo)
                else
                    ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("/code " .. codigo, "All")
                end
            end)
            task.wait(1.5)
        end
    end)
end })

