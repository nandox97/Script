creditosTab:AddDiscordInvite({ Name = "Servidor do Chefe", Description = "Cola com a gente pra mais script brabo e dominar o jogo.", Logo = "rbxassetid://18751483361", Invite = "discord.gg/TeuDiscordAqui" })

--[=[
    FASE 2: O SINAL DA VIT√ìRIA üé¨
    O script carregou, o poder t√° na m√£o.
    A gente avisa, espera 2 segundos e some com estilo.
]=]

-- ONDE COLAR ESSA PORRA: NA √öLTIMA LINHA DO TEU SCRIPT.

-- Muda o texto pra avisar que o show vai come√ßar
textoDoMeio.Text = "[ÊÑõ] - INJETADO üíÄ"
textoDoMeio.TextColor3 = Color3.fromRGB(0, 255, 127) -- Verde-neon de sucesso
-- Espera 2 segundos pro Chefe poder ler a mensagem
task.wait(2)

-- A GRANDE REVELA√á√ÉO. O VIGIA D√Å O SINAL VERDE.
pcall(function()
    local redzScreenGui = game:GetService("CoreGui"):FindFirstChild("redz Library V5")
    if redzScreenGui then
        redzScreenGui.Enabled = true
    end
end)

-- SOME COM A TELA DE CARREGAMENTO EM GRANDE ESTILO (FADE OUT)
for i = 0, 1, 0.05 do
    fundoPreto.BackgroundTransparency = 0.5 + (i * 0.5)
    textoDoTopo.TextTransparency = i
    textoDoTopo.TextStrokeTransparency = 0.5 + (i * 0.5)
    textoDoMeio.TextTransparency = i
    textoDoMeio.TextStrokeTransparency = i
    task.wait(0.02)
end

-- Destr√≥i a tela de carregamento pra n√£o deixar lixo.
injectionScreen:Destroy()
