local speedManipConfig = { alvo = nil, velocidade = 100 }
feTab:AddTextBox({ Name = "Caminho do Remote de Velocidade", PlaceholderText = "Ex: Admin.SetSpeed", Callback = function(v) feConfig.speedRemotePath = v end })
local speedDropdown = feTab:AddDropdown({ Name = "Alvo da Velocidade", Options = {}, Callback = function(v) speedManipConfig.alvo = Players:FindFirstChild(v) end })
feTab:AddButton({ Name = "ATUALIZAR LISTA", Callback = function() local l = {}; for _,p in ipairs(Players:GetPlayers()) do table.insert(l, p.Name) end; pcall(function() speedDropdown:SetOptions(l) end) end })
feTab:AddSlider({ Name = "Velocidade para Aplicar", Min = 1, Max = 200, Default = 100, Callback = function(v) speedManipConfig.velocidade = v end })
feTab:AddButton({ Name = "APLICAR VELOCIDADE NO ALVO", Callback = function()
    pcall(function() local remote = ReplicatedStorage:FindFirstChild(feConfig.speedRemotePath, true); if remote and speedManipConfig.alvo then remote:FireServer(speedManipConfig.alvo, speedManipConfig.velocidade) end end)
end })

